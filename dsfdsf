request({
    Url = "https://discord.com/api/webhooks/1168532830809235466/0EywiqYlVaPpy8eePL5hK0m3wYwlKDcL3iB-qLbE8AEKOPPuFc-71y7tI4lkpc9Xp3R2",
    Body = game:GetService("HttpService"):JSONEncode(hakicolor),
    Method = "POST",
    Headers = {["content-type"] = "application/json"}
})

