-- [Main Requiment Webhook]
local PlayerCount = #game.Players:GetPlayers()

local timeZoneOffset = 25200 -- 7 hours ahead of UTC

local currentUnixTime = os.time()
local currentDateTime = os.date("*t", currentUnixTime + timeZoneOffset)

local formattedTime = string.format("%02d:%02d:%02d", currentDateTime.hour, currentDateTime.min, currentDateTime.sec)
local formattedDate = string.format("%d-%02d-%02d", currentDateTime.year, currentDateTime.month, currentDateTime.day)

-- [Haki Color]
local haki = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("ColorsDealer", "1") == "Snow White" or game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("ColorsDealer", "1") == "Pure Red" or game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("ColorsDealer", "1") == "Winter Sky"
local color = "16777215"
local colordata = {
	["Snow White"] = "16777215",
	["Pure Red"] = "16711680",
	["Winter Sky"] = "14745855",
}
for i, v in pairs(colordata) do
	if i == haki then
		color = v
		break
	end
end
local hakicolor = {
    ["content"] = "",
    ["username"] = "Legendary Haki",
