		Options.AutoFactoryHop:OnChanged(function()
			_G.Settings.Main["Auto Factory Hop"] = Options["AutoFactoryHop"].Value
			SaveSettings()
		end)
	
		spawn(function()
			while wait() do
				if _G.Settings.Main["Auto Factory"] then
					pcall(function()
						if game:GetService("Workspace").Enemies:FindFirstChild("Core") then
							if Auto_Farm_Level then _G.Settings.Main["Auto Farm Level"] = false end
							for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
								if v.Name == "Core" and v.Humanoid.Health > 0 then
									repeat wait()
										AutoHaki()         
										EquipWeapon(_G.Settings.Config["Select Weapon"])  
										FastAttack = true          
										toTarget(CFrame.new(448.46756, 199.356781, -441.389252))
										if not _G.Settings.Config["Fast Attack"] then                                  
											game:GetService("VirtualUser"):CaptureController()
											game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
										end
									until v.Humanoid.Health <= 0 or _G.Settings.Main["Auto Factory"] == false
									FastAttack = false
								end
							end
						else
							if game:GetService("ReplicatedStorage"):FindFirstChild("Core") then
								toTarget(CFrame.new(448.46756, 199.356781, -441.389252))
							else 
								if _G.Settings.Main["Auto Factory Hop"] then
									Hop()
								end
							end
						end
					end)
				end
			end
		end)

		OtherFunction:AddToggle("AutoEvo", {Title = "Auto Race V2 Quest", Default = _G.Settings.Main["Evo Race V2"]})
		Options.AutoEvo:OnChanged(function()
			_G.Settings.Main["Evo Race V2"] = Options["AutoEvo"].Value
			if Options["AutoEvo"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
