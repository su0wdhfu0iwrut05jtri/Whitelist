																		game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
																		wait(.1)
																		game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
																	end
																elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
																	game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).MousePos.Value = v.HumanoidRootPart.Position
																	if _G.Settings.Main["Use Skill"].Z == true and MasterySkill.Z.Cooldown.AbsoluteSize.X <= 1 and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.Z then
																		repeat wait()
																			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Main["Distance Mastery"], 5)
