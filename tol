			Title = "Bone Checker",
			Content = ""
		})

		BoneTab:AddToggle("AutoBone", {Title = "Auto Bone Farm", Default = _G.Settings.Main["Auto Bone"] })
		Options.AutoBone:OnChanged(function()
			_G.Settings.Main["Auto Bone"] = Options["AutoBone"].Value
			BoneText:SetDesc("You Have Bones : "..(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Bones","Check")))
			if Options["AutoBone"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)

		BoneTab:AddToggle("RandomSuprise", {Title = "Auto Surprise Stop When Got Essence", Default = _G.Settings.Main["Random Suprise"]})
