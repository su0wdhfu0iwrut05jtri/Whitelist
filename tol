					if i == 12 then
						PlayerStatus:SetDesc("Players :".." "..i.." ".."/".." ".."12".." ".."(Max)")
					elseif i == 1 then
						PlayerStatus:SetDesc("Player :".." "..i.." ".."/".." ".."12")
					else
						PlayerStatus:SetDesc("Players :".." "..i.." ".."/".." ".."12")
					end
				end
			end)
		end
	end)
	
