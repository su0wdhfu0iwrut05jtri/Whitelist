																		if _G.Settings.Main["Use Skill"].C == true and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.C then
																			game:service('VirtualInputManager'):SendKeyEvent(true, "C", false, game)
																			wait(.1)
																			game:service('VirtualInputManager'):SendKeyEvent(false, "C", false, game)
																		end  
																	elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("Control-Control") then
																		if _G.Settings.Main["Use Skill"].Z == true and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and game:GetService("Workspace")["_WorldOrigin"]:FindFirstChild("Globe") and (game:GetService("Workspace")["_WorldOrigin"]:FindFirstChild("Globe").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 50 and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.Z then
																		
																		else
																			game:service('VirtualInputManager'):SendKeyEvent(true, "Z", false, game)
																			wait(.1)
																			game:service('VirtualInputManager'):SendKeyEvent(false, "Z", false, game)
																		end
																		if _G.Settings.Main["Use Skill"].X == true and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and game:GetService("Workspace")["_WorldOrigin"]:FindFirstChild("Globe") and (game:GetService("Workspace")["_WorldOrigin"]:FindFirstChild("Globe").Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 50 and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.X then
																			game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
																			wait(.1)
																			game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
																		end
																	elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
																		game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).MousePos.Value = v.HumanoidRootPart.Position
																		if _G.Settings.Main["Use Skill"].Z == true and MasterySkill.Z.Cooldown.AbsoluteSize.X <= 1 and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.Z then
																			repeat wait()
																				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Main["Distance Mastery"], 5)
																				game:service('VirtualInputManager'):SendKeyEvent(true, "Z", false, game)
																				if _G.Settings.Main["Hold Skill"].Z == true then
																					wait(2)
																				else
																					wait(.1)
																				end
																				game:service('VirtualInputManager'):SendKeyEvent(false, "Z", false, game)
																			until MasterySkill.Z.Cooldown.AbsoluteSize.X >= 1 or game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Holding.Value == false
																		else
																			if v:FindFirstChild("HumanoidRootPart") or not v:FindFirstChild("HumanoidRootPart") then
																				repeat wait()
																					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 150, 0)
																				until MasterySkill.X.Cooldown.AbsoluteSize.X <= 1 or game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Holding.Value == false
																			end
																		end
																		if _G.Settings.Main["Use Skill"].X == true and MasterySkill.X.Cooldown.AbsoluteSize.X <= 1 and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.X then
																			repeat wait()
																				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Main["Distance Mastery"], 5)
																				game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
																				if _G.Settings.Main["Hold Skill"].X == true then
																					wait(2)
																				else
																					wait(.1)
																				end
																				game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
																			until MasterySkill.X.Cooldown.AbsoluteSize.X >= 1 or game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Holding.Value == false
																		else
																			if v:FindFirstChild("HumanoidRootPart") or not v:FindFirstChild("HumanoidRootPart") then
																				repeat wait()
																					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 150, 0)
																				until MasterySkill.X.Cooldown.AbsoluteSize.X <= 1 or game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Holding.Value == false
																			end
																		end
																		if _G.Settings.Main["Use Skill"].C == true and MasterySkill.C.Cooldown.AbsoluteSize.X <= 1 and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.C then
																			repeat wait()
																				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Main["Distance Mastery"], 5)
																				game:service('VirtualInputManager'):SendKeyEvent(true, "C", false, game)
																				if  _G.Settings.Main["Hold Skill"].C == true then
																					wait(2)
																				else
																					wait(.1)
																				end
																				game:service('VirtualInputManager'):SendKeyEvent(false, "C", false, game)
																			until MasterySkill.C.Cooldown.AbsoluteSize.X >= 1 or game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Holding.Value == false
																		else
																			if v:FindFirstChild("HumanoidRootPart") or not v:FindFirstChild("HumanoidRootPart") then
																				repeat wait()
																					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 150, 0)
																				until MasterySkill.C.Cooldown.AbsoluteSize.X <= 1 or game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Holding.Value == false
																			end
																		end
																		if _G.Settings.Main["Use Skill"].V == true and MasterySkill.V.Cooldown.AbsoluteSize.X <= 1 and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and DevilFruitMastery >= MasteryDevilFruit.Lvl.V then
																			repeat wait()
																				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Main["Distance Mastery"], 5)
																				game:service('VirtualInputManager'):SendKeyEvent(true, "V", false, game)
																				if  _G.Settings.Main["Hold Skill"].V == true then
																					wait(2)
																				else
																					wait(.1)
																				end
																				game:service('VirtualInputManager'):SendKeyEvent(false, "V", false, game)
																			until MasterySkill.V.Cooldown.AbsoluteSize.X >= 1 or game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Holding.Value == false
																		else
																			if v:FindFirstChild("HumanoidRootPart") or not v:FindFirstChild("HumanoidRootPart") then
																				repeat wait()
																					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 150, 0)
																				until MasterySkill.V.Cooldown.AbsoluteSize.X <= 1 or game:GetService("Players").LocalPlayer.Character:FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value).Holding.Value == false
																			end
																		end
																	end
																end
															else
																AutoHaki()
																if not _G.Settings.Config["Fast Attack"] then
																	game:GetService('VirtualUser'):CaptureController()
																	game:GetService('VirtualUser'):ClickButton1(Vector2.new(851, 158), game:GetService("Workspace").Camera.CFrame)
																end
																UseSkillMasteryDevilFruit = false
																FastAttack = true
																StartMagnet = true
																AutoHaki()
																EquipWeapon(_G.Settings.Config["Select Weapon"]) 
																--v.HumanoidRootPart.Size = Vector3.new(60,60,60)
																toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 10))
															end
														end
													until not _G.Settings.Main["Auto Farm Fruit Mastery"] or v.Humanoid.Health <= 0 or not v.Parent or v.Humanoid.Health <= 0
													StartMagnet = false
													FastAttack = false
												end
											end
										end
									else
										StartMagnet = false
										toTarget(CFrame.new(-1820.0634765625, 210.74781799316406, -12297.49609375))
										if game:GetService("ReplicatedStorage"):FindFirstChild("Cookie Crafter") then
											toTarget(game:GetService("ReplicatedStorage"):FindFirstChild("Cookie Crafter").HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0)) 
										else
											if game:GetService("ReplicatedStorage"):FindFirstChild("Cake Guard") then
												toTarget(game:GetService("ReplicatedStorage"):FindFirstChild("Cake Guard").HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0)) 
											else
												if game:GetService("ReplicatedStorage"):FindFirstChild("Baking Staff") then
													toTarget(game:GetService("ReplicatedStorage"):FindFirstChild("Baking Staff").HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
												else
													if game:GetService("ReplicatedStorage"):FindFirstChild("Head Baker") then
														toTarget(game:GetService("ReplicatedStorage"):FindFirstChild("Head Baker").HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
													end
												end
											end
										end                       
									end
								else
									if game:GetService("Workspace").Enemies:FindFirstChild("Cake Prince") then
										toTarget(game:GetService("Workspace").Enemies:FindFirstChild("Cake Prince").HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
									else
										if game:GetService("ReplicatedStorage"):FindFirstChild("Cake Prince") then
											toTarget(game:GetService("ReplicatedStorage"):FindFirstChild("Cake Prince").HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
										end
									end
								end
							end
						end
					end
				end
			end)
		end
	end)
	
	local MonsterTab = Tabs.MonsterFunction:AddSection("[ Select Monster ]")
	if World1 then
		tableMon = {"Bandit [Lv. 5]","Monkey [Lv. 14]","Gorilla [Lv. 20]","Pirate [Lv. 35]","Brute [Lv. 45]","Desert Bandit [Lv. 60]","Desert Officer [Lv. 70]","Snow Bandit [Lv. 90]","Snowman [Lv. 100]","Chief Petty Officer [Lv. 120]","Sky Bandit [Lv. 150]","Dark Master [Lv. 175]","Toga Warrior [Lv. 225]","Gladiator [Lv. 275]","Military Soldier [Lv. 300]","Military Spy [Lv. 330]","Fishman Warrior [Lv. 375]","Fishman Commando [Lv. 400]","God's Guard [Lv. 450]","Shanda [Lv. 475]","Royal Squad [Lv. 525]","Royal Soldier [Lv. 550]","Galley Pirate [Lv. 625]","Galley Captain [Lv. 650]"}
	elseif World2 then
		tableMon = {"Raider [Lv. 700]","Mercenary [Lv. 725]","Swan Pirate [Lv. 775]","Factory Staff [Lv. 800]","Marine Lieutenant [Lv. 875]","Marine Captain [Lv. 900]","Zombie [Lv. 950]","Vampire [Lv. 975]","Snow Trooper [Lv. 1000]","Winter Warrior [Lv. 1050]","Lab Subordinate [Lv. 1100]","Horned Warrior [Lv. 1125]","Magma Ninja [Lv. 1175]","Lava Pirate [Lv. 1200]","Ship Deckhand [Lv. ]","Ship Engineer [Lv. ]","Ship Steward [Lv. ]","Ship Officer [Lv. 1325]","Arctic Warrior [Lv. 1350]","Snow Lurker [Lv. 1375]","Sea Soldier [Lv. 1425]","Water Fighter [Lv. 1450]"}
	elseif World3 then
		tableMon = {"Pirate Millionaire [Lv. 1500]","Pistol Billionaire [Lv. 1525]","Dragon Crew Warrior [Lv. 1575]","Dragon Crew Archer [Lv. 1600]","Female Islander [Lv. 1625]","Giant Islander [Lv. 1650]","Marine Commodore [Lv. 1700]","Marine Rear Admiral [Lv. 1725]","Fishman Raider [Lv. 1775]","Fishman Captain [Lv. 1800]","Forest Pirate [Lv. 1825]","Mythological Pirate [Lv. 1850]","Jungle Pirate [Lv. 1900]","Musketeer Pirate [Lv. 1925]","Reborn Skeleton [Lv. 1975]","Living Zombie [Lv. 2000]","Demonic Soul [Lv. 2025]","Posessed Mummy [Lv. 2050]" ,"Peanut Scout [Lv. 2075]","Peanut President [Lv. 2100]","Ice Cream Chef [Lv. 2125]","Ice Cream Commander [Lv. 2150]","Cookie Crafter [Lv. 2200]","Cake Guard [Lv. 2225]","Baking Staff [Lv. 2250]","Head Baker [Lv. 2275]","Cocoa Warrior [Lv. 2300]","Chocolate Bar Battler [Lv. 2325]","Sweet Thief [Lv. 2350]","Candy Rebel [Lv. 2375]","Candy Pirate [Lv. 2400]","Snow Demon [Lv. 2425]","Isle Outlaw [Lv. 2450]","Island Boy [Lv. 2475]","Sun-kissed Warrior [Lv. 2500]","Isle Champion [Lv. 2525]"}
	end
	SelectMonster = ""
	MonsterTab:AddDropdown("MonsterSelector", {
		Title = "Select Monster to Farm",
		Values = tableMon,
		Multi = false,
		Default = SelectMonster,
	})
	
	Options.MonsterSelector:OnChanged(function()
		SelectMonster = Options["MonsterSelector"].Value
		SaveSettings()
	end)
	
	MonsterTab:AddToggle("AutoSelectMOnster", {Title = "Auto Farm Selected Monster", Default = _G.Settings.Main["Auto Farm Select Monster"]})
	Options.AutoSelectMOnster:OnChanged(function()
		_G.Settings.Main["Auto Farm Select Monster"] = Options["AutoSelectMOnster"].Value
		if Options["AutoSelectMOnster"].Value == false then
			wait()
			toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
			wait()
		end
		SaveSettings()
	end)

	local BossFunction = Tabs.MonsterFunction:AddSection("[ Bosses Farm ]")

	local Boss = {}
	for i, v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
		if v.Name == "rip_indra" or v.Name == "Ice Admiral" then
		
		--World1 
		elseif v.Name == "Saber Expert" or v.Name == "The Saw" or v.Name == "Greybeard" or v.Name == "Mob Leader" or v.Name == "The Gorilla King" or v.Name == "Bobby" or v.Name == "Yeti" or v.Name == "Vice Admiral" or v.Name == "Warden" or v.Name == "Chief Warden" or v.Name == "Swan" or v.Name == "Magma Admiral" or v.Name == "Fishman Lord" or v.Name == "Wysper" or v.Name == "Thunder God" or v.Name == "Cyborg"
		--World2
		or v.Name == "Don Swan" or v.Name == "Diamond"
		--World3
		or v.Name == "Stone" or v.Name == "Island Empress" or v.Name == "Kilo Admiral" or v.Name == "Captain Elephant" or v.Name == "Beatiful Pirate" or v.Name == "Cake Queen" or v.Name == "rip_indra True Form" or v.Name == "Longma" or v.Name == "Soul Reaper" or v.Name == "Cake Prince" or v.Name == "Dough King"
		then
			table.insert(Boss, v.Name)
		end
	end

	_G.SelectBoss = ""
	BossFunction:AddDropdown("BossSelector", {
		Title = "Select Boss",
		Values = Boss,
		Multi = false,
		Default = SelectBoss,
	})
	
	Options.BossSelector:OnChanged(function()
		_G.SelectBoss = Options["BossSelector"].Value
	end)

	game:GetService("ReplicatedStorage").ChildAdded:Connect(
		function(boss)
			if boss.Name == "Saber Expert" or boss.Name == "The Saw" or boss.Name == "Greybeard" or boss.Name == "Mob Leader" or boss.Name == "The Gorilla King" or boss.Name == "Bobby" or boss.Name == "Yeti" or boss.Name == "Vice Admiral" or boss.Name == "Warden" or boss.Name == "Chief Warden" or boss.Name == "Swan" or boss.Name == "Magma Admiral" or boss.Name == "Fishman Lord" or boss.Name == "Wysper" or boss.Name == "Thunder God" or boss.Name == "Cyborg"
			--World2
			or boss.Name == "Don Swan" or boss.Name == "Diamond"
			--World3
			or boss.Name == "Stone" or boss.Name == "Island Empress" or boss.Name == "Kilo Admiral" or boss.Name == "Captain Elephant" or boss.Name == "Beatiful Pirate" or boss.Name == "Cake Queen" or boss.Name == "rip_indra True Form" or boss.Name == "Longma" or boss.Name == "Soul Reaper" or boss.Name == "Cake Prince" or boss.Name == "Dough King" 
			then
				table.insert(Boss, boss.Name)
			end
			Options.BossSelector:SetValues()
		end
	)
	game:GetService("ReplicatedStorage").ChildRemoved:Connect(
		function(boss)
			if boss.Name == "Saber Expert" or boss.Name == "The Saw" or boss.Name == "Greybeard" or boss.Name == "Mob Leader" or boss.Name == "The Gorilla King" or boss.Name == "Bobby" or boss.Name == "Yeti" or boss.Name == "Vice Admiral" or boss.Name == "Warden" or boss.Name == "Chief Warden" or boss.Name == "Swan" or boss.Name == "Magma Admiral" or boss.Name == "Fishman Lord" or boss.Name == "Wysper" or boss.Name == "Thunder God" or boss.Name == "Cyborg"			--World2
			or boss.Name == "Don Swan" or boss.Name == "Diamond"
			--World3
			or boss.Name == "Stone" or boss.Name == "Island Empress" or boss.Name == "Kilo Admiral" or boss.Name == "Captain Elephant" or boss.Name == "Beatiful Pirate" or boss.Name == "Cake Queen" or boss.Name == "rip_indra True Form" or boss.Name == "Longma" or boss.Name == "Soul Reaper" or boss.Name == "Cake Prince" or boss.Name == "Dough King" 
			then
				for i,v in pairs(Boss) do
					if v == boss.Name and not game:GetService("ReplicatedStorage"):FindFirstChild(boss.Name) then
						table.remove(Boss, i)
					end
				end
				Options.BossSelector:SetValues()
			end
		end
	)

	BossFunction:AddToggle("AutoFFarmBoss", {Title = "Auto Kill Selected Boss", Default = _G.Settings.Main["Auto Farm Boss"]})
	Options.AutoFFarmBoss:OnChanged(function()
		_G.Settings.Main["Auto Farm Boss"] = Options["AutoFFarmBoss"].Value
		if Options["AutoFFarmBoss"].Value == false then
			wait()
			toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
			wait()
		end
		SaveSettings()
	end)
	spawn(function()
		while wait() do
			if _G.Settings.Main["Auto Farm Boss"] then
				pcall(function()
					CheckQuestBoss()
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", QuestNameBoss, LevelQuestBoss)
					if game:GetService("Workspace").Enemies:FindFirstChild(_G.SelectBoss) then
						for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
							if v.Name == _G.SelectBoss then
								if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
									repeat wait()
										AutoHaki()
										EquipWeapon(_G.Settings.Config["Select Weapon"])
										v.HumanoidRootPart.CanCollide = false
										FastAttack = true
										v.Humanoid.WalkSpeed = 0                           
										toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
										if not _G.Settings.Config["Fast Attack"] then
											game:GetService("VirtualUser"):CaptureController()
											game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
										end
									until not _G.Settings.Main["Auto Farm Boss"] or not v.Parent or v.Humanoid.Health <= 0
									FastAttack = false
								end
							end
						end
					else
						FastAttack = false
						if game:GetService("ReplicatedStorage"):FindFirstChild(_G.SelectBoss) then
							if Auto_Farm_Level then _G.Settings.Main["Auto Farm Level"] = false end
							toTarget(game:GetService("ReplicatedStorage"):FindFirstChild(_G.SelectBoss).HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
						end
					end
				end)
			end
		end
	end)

	if World3 then
		local KatakuriTab = Tabs.MonsterFunction:AddSection("[ Katakuri / Dough Boss ]")

		MobKilled = KatakuriTab:AddParagraph({
			Title = "Monster Killed Checker",
			Content = ""
		})
	
		spawn(function()
			while wait() do
				pcall(function()
					if string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 88 then
						MobKilled:SetDesc("Defeat : "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,41))
					elseif string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 87 then
						MobKilled:SetDesc("Defeat : "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,40))
					elseif string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 86 then
						MobKilled:SetDesc("Defeat : "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,39))
					else
						MobKilled:SetDesc("Boss Is Spawning")
					end
				end)
			end
		end)
		
		KatakuriTab:AddToggle("AutoKatakuriVersi1", {Title = "Auto Katakuri V1", Default = _G.Settings.Main["Auto KatakuriV1"]})
		Options.AutoKatakuriVersi1:OnChanged(function()
			_G.Settings.Main["Auto KatakuriV1"] = Options["AutoKatakuriVersi1"].Value
			if Options["AutoKatakuriVersi1"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)
	
		KatakuriTab:AddToggle("AutoKatakuriVersi2", {Title = "Auto Katakuri V2", Default = _G.Settings.Main["Auto KatakuriV2"]})
		Options.AutoKatakuriVersi2:OnChanged(function()
			_G.Settings.Main["Auto KatakuriV2"] = Options["AutoKatakuriVersi2"].Value
			if Options["AutoKatakuriVersi2"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)
	
		local EliteHunterTab = Tabs.MonsterFunction:AddSection("[ Elite Hunter ]")
	
		EliteSpawn = EliteHunterTab:AddParagraph({
			Title = "",
			Content = ""
		})
	
		spawn(function()
			pcall(function()
				while wait() do
					if game:GetService("ReplicatedStorage"):FindFirstChild("Urban") or game:GetService("ReplicatedStorage"):FindFirstChild("Deandre") or game:GetService("ReplicatedStorage"):FindFirstChild("Diablo") then
						EliteSpawn:SetTitle("Elite NPC Status : Spawn!")
					else
						EliteSpawn:SetTitle("Elite NPC Status : Not Spawn!")
					end
					EliteSpawn:SetDesc("Elite Progress : "..game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EliteHunter","Progress"))
				end
			end)
		end)
	
		EliteHunterTab:AddToggle("Elite", {Title = "Auto Elite Hunter", Default = _G.Settings.Main["Auto Elite Hunter"]})
		Options.Elite:OnChanged(function()
			_G.Settings.Main["Auto Elite Hunter"] = Options["Elite"].Value
			if Options["Elite"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)
	
		EliteHunterTab:AddToggle("EliteCup", {Title = "Stop Hop when got Chalice", Default = _G.Settings.Main["Elite Cup"]})
		Options.EliteCup:OnChanged(function()
			_G.Settings.Main["Elite Cup"] = Options["EliteCup"].Value
			SaveSettings()
		end)

		EliteHunterTab:AddToggle("EliteHop", {Title = "Elite Hunter Hop", Default = _G.Settings.Main["Elite Hop"]})
		Options.EliteHop:OnChanged(function()
			_G.Settings.Main["Elite Hop"] = Options["EliteHop"].Value
			SaveSettings()
		end)

		spawn(function()
			while wait() do
				pcall(function()
					if _G.Settings.Main["Auto Elite Hunter"] then
						if game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true then
							if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Diablo") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Urban") or string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Deandre") then
								for i,v in pairs(game.ReplicatedStorage:GetChildren()) do
									if string.find(v.Name,"Diablo") or string.find(v.Name,"Urban") or string.find(v.Name,"Deandre") then
										if Auto_Farm_Level then _G.Settings.Main["Auto Farm Level"] = false end
										EliteHunter = toTarget(v.HumanoidRootPart.CFrame)
										if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 150 then
											if EliteHunter then
												EliteHunter:Stop()
											end
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
										end
									end	
								end
								for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
									if string.find(v.Name,"Diablo") or string.find(v.Name,"Urban") or string.find(v.Name,"Deandre") then
										if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
											repeat wait()
												AutoHaki()
												if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude >150 then
													Farmtween = toTarget(v.HumanoidRootPart.CFrame)
												elseif (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 150 then
													if Farmtween then
														Farmtween:Stop()
													end
													game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 1)
													if not _G.Settings.Config["Fast Attack"] then
														game:GetService("VirtualUser"):CaptureController()
														game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
													end
													FastAttack = true
													EquipWeapon(_G.Settings.Config["Select Weapon"]) 
												end 
											until not _G.Settings.Main["Auto Elite Hunter"] or not v.Parent or v.Humanoid.Health <= 0
											FastAttack = false
										end
									end
								end
							else
								local string_1 = "EliteHunter";
								local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
								Target:InvokeServer(string_1);
							end
						else
							local string_1 = "EliteHunter";
							local Target = game:GetService("ReplicatedStorage").Remotes["CommF_"];
							Target:InvokeServer(string_1);
						end
					end
					if _G.Settings.Main["Elite Hop"] and _G.Settings.Main["Auto Elite Hunter"] then
						if not game.Players.LocalPlayer.Backpack:FindFirstChild("God's Chalice") or not game.Players.LocalPlayer.Character:FindFirstChild("God's Chalice") then
							if game:GetService("ReplicatedStorage").Remotes["CommF_"]:InvokeServer("EliteHunter") == "I don't have anything for you right now. Come back later." then
								Hop()
							end
						elseif game.Players.LocalPlayer.Backpack:FindFirstChild("God's Chalice") or game.Players.LocalPlayer.Character:FindFirstChild("God's Chalice") and _G.Settings.Main["Elite Cup"] then
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-12471.169921875, 374.94024658203, -7551.677734375))
							if Auto_Farm_Level then _G.Settings.Main["Auto Farm Level"] = false end
						end
					end
				end)
			end
		end)
	else
		_G.Settings.Main["Auto Elite Hunter"] = false
		_G.Settings.Main["Elite Cup"] = false
		_G.Settings.Main["Elite Hop"] = false
	end

	if World1 then
		BossItemTable = {"Shark Saw - The Saw [Lv. 100] [Boss]","Vice Admiral's Coat - Vice Admiral [Lv. 130] [Boss]","Wardens Sword - Chief Warden [Lv. 200] [Boss]","Pink Coat - Swan [Lv. 225] [Boss]","Refined Musket - Magma Admiral [Lv. 350] [Boss]","Trident - Fishman Lord [Lv. 425] [Boss]","Pole - Thunder God [Lv. 575] [Boss]","Cool Shades - Cyborg [Lv. 675] [Boss]"}
	elseif World2 then
		BossItemTable = {"Longsword - Diamond [Lv. 750] [Boss]","Black Spikey Coat - Jeremy [Lv. 800] [Boss]","Gravity Cane - Fajita [Lv. 925] [Boss]","Jitte - Smoke Admiral [Lv. 1150] [Boss]","Dragon Trident - Tide Keeper [Lv. 1475] [Boss]"}
	elseif World3 then
		BossItemTable = {"Pilot Helmet - Stone [Lv. 1550] [Boss]","Serpent Bow - Island Empress [Lv. 1675] [Boss]","Lei - Kilo Admiral [Lv. 1750] [Boss]","Twin Hooks - Captain Elephant [Lv. 1875] [Boss]","Canvander - Beautiful Pirate [Lv. 1950] [Boss]","Buddy Sword - Cake Queen [Lv. 2175] [Boss]"}
	end
	local BossDrop = Tabs.ItemFunction:AddSection("[ Boss IItems Drops ]")
	BossDrop:AddDropdown("ItemBossSelector", {
		Title = "Select Items to Get",
		Values = BossItemTable,
		Multi = false,
		Default = _G.Settings.Main["Select Item"],
	})
	Options.ItemBossSelector:OnChanged(function()
		_G.Settings.Main["Select Item"] = Options["ItemBossSelector"].Value
		SaveSettings()
	end)

	BossDrop:AddToggle("DropBoss", {Title = "Enable Items to Get", Default = _G.Settings.Main["Items Drop"]})
	Options.DropBoss:OnChanged(function()
		_G.Settings.Main["Items Drop"] = Options["DropBoss"].Value
		if Options["DropBoss"].Value == false then
			wait()
			toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
			wait()
		end
		SaveSettings()
	end)

	BossDrop:AddToggle("DropBossHop", {Title = "Enable Items to Get Hop", Default = _G.Settings.Main["Hop Item Drop"]})
	Options.DropBossHop:OnChanged(function()
		_G.Settings.Main["Hop Item Drop"] = Options["DropBossHop"].Value
		SaveSettings()
	end)

	spawn(function()
		while wait() do
			if _G.Settings.Main["Items Drop"] then
				pcall(function()
					if World1 and _G.Settings.Main["Select Item"] == "Shark Saw - The Saw [Lv. 100] [Boss]" then
						if game:GetService("Workspace").Enemies:FindFirstChild("The Saw") then
							for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
								if v.Name == "The Saw" then
									repeat wait()
										StartMagnet = true
										FastAttack = true
										AutoHaki()
										EquipWeapon(_G.Settings.Config["Select Weapon"]) 
										toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
										PosMon = v.HumanoidRootPart.CFrame
										if not _G.Settings.Config["Fast Attack"] then
											game:GetService'VirtualUser':CaptureController()
											game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
										end
										--v.HumanoidRootPart.Size = Vector3.new(60,60,60)
										v.HumanoidRootPart.Transparency = 1
										v.Humanoid.JumpPower = 0
										v.Humanoid.WalkSpeed = 0
										v.HumanoidRootPart.CanCollide = false
										v.Humanoid:ChangeState(11)
									until v.Humanoid.Health <= 0 or _G.Settings.Main["Items Drop"] == false
								end
							end
						else
							if game:GetService("ReplicatedStorage"):FindFirstChild("The Saw") then
								if Auto_Farm_Level then _G.Settings.Main["Auto Farm Level"] = false end
								toTarget(game:GetService("ReplicatedStorage"):FindFirstChild("The Saw").HumanoidRootPart.CFrame * CFrame.new(5,10,7))
							else
								if _G.Settings.Main["Hop Item Drop"] and World1 then
									wait(1)
									Hop()
								end
								wait(2)
								Fluent:Notify({
									Title = "Boss Status",
									Content = "The Saw",
									SubContent = "Not Spawn Yet !",
									Duration = 3
								})
							end
						end
					elseif World1 and _G.Settings.Main["Select Item"] == "Vice Admiral's Coat - Vice Admiral [Lv. 130] [Boss]" then
						if game:GetService("Workspace").Enemies:FindFirstChild("Vice Admiral") then
							for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
								if v.Name == "Vice Admiral" then
									repeat wait()
										StartMagnet = true
