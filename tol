		end)
	
		LegendarySword:AddToggle("AutoLegendarySwordHop", {Title = "Auto Legendary Sword Hop", Default = _G.Settings.Main["Auto Legendary Sword Dealer Hop"]})
		Options.AutoLegendarySwordHop:OnChanged(function()
			_G.Settings.Main["Auto Legendary Sword Dealer Hop"] = Options["AutoLegendarySwordHop"].Value
			SaveSettings()
		end)
	
		spawn(function()
			while wait() do
				if _G.Settings.Main["Auto Legendary Sword Dealer"] and World2 then
					pcall(function()
						--if _G.Settings.Main["Legendary Sword List"].Saddi == true then
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("LegendarySwordDealer","1")
						--end
					--	if _G.Settings.Main["Legendary Sword List"].Shisui == true then
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("LegendarySwordDealer","2")
						--end
						--if _G.Settings.Main["Legendary Sword List"].Wando == true then
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("LegendarySwordDealer","3")
						--end
						if _G.Settings.Main["Auto Legendary Sword Dealer Hop"] and _G.Settings.Main["Auto Legendary Sword Dealer"] and World2 then
							wait(5)
							Hop()
						end
					end)
				end 
			end
		end)

	else
		_G.Settings.Main["Auto Legendary Sword Dealer"] = false
		_G.Settings.Main["Auto Legendary Sword Dealer Hop"] = false
	end

	if World3 then
		local CDKSection = Tabs.ItemFunction:AddSection("[ Cursed Dual Katana ]")
		CDKSection:AddToggle("Cursed", {Title = "Auto CDK Quest Full", Default = _G.Settings.Sword["CDK"]})
		Options.Cursed:OnChanged(function()
			_G.Settings.Sword["CDK"] = Options["Cursed"].Value
			if Options["Cursed"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)

		spawn(function()
			while wait() do
				pcall(function()
					for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
						if StartMagnet and v:FindFirstChild("HazeESP") and (v.HumanoidRootPart.Position - PosMonsEsp.Position).magnitude <= 300 then
							if InMyNetWork(v.HumanoidRootPart) then
								v.HumanoidRootPart.CFrame = PosMonsEsp
								v.Humanoid.JumpPower = 0
								v.Humanoid.WalkSpeed = 0
								--v.HumanoidRootPart.Size = Vector3.new(60,60,60)
								v.HumanoidRootPart.Transparency = 1
								v.HumanoidRootPart.CanCollide = false
								v.Head.CanCollide = false
								if v.Humanoid:FindFirstChild("Animator") then
									v.Humanoid.Animator:Destroy()
								end
								v.Humanoid:ChangeState(11)
								v.Humanoid:ChangeState(14)
								sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
								if not v.HumanoidRootPart:FindFirstChild("BodyVelocity") then
									local vc = Instance.new("BodyVelocity", v.HumanoidRootPart)
									vc.MaxForce = Vector3.new(1, 1, 1) * math.huge
									vc.Velocity = Vector3.new(0, 0, 0)
								end
							end
						end
					end
				end)
			end
		end)

		spawn(function()
			while wait() do
				pcall(function()
					if _G.Settings.Sword["CDK"] and World3 then
						if GetWeaponInventory("Cursed Dual Katana") == false then
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CDKQuest","StartTrial","Good")
							wait(.1)
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CDKQuest","StartTrial","Evil")
							for i,v in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CDKQuest","Progress")) do
								if i == "Good" and v == -3 then
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CDKQuest","BoatQuest",workspace.NPCs:FindFirstChild("Luxury Boat Dealer"))
									BTP(CFrame.new(-9546.990234375, 21.139892578125, 4686.1142578125))
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CDKQuest","BoatQuest",workspace.NPCs:FindFirstChild("Luxury Boat Dealer"))
									wait(5)
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CDKQuest","BoatQuest",workspace.NPCs:FindFirstChild("Luxury Boat Dealer"))
									BTP(CFrame.new(-6120.0576171875, 16.455780029296875, -2250.697265625))
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CDKQuest","BoatQuest",workspace.NPCs:FindFirstChild("Luxury Boat Dealer"))
									wait(5)
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CDKQuest","BoatQuest",workspace.NPCs:FindFirstChild("Luxury Boat Dealer"))
									BTP(CFrame.new(-9533.2392578125, 7.254445552825928, -8372.69921875))  
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CDKQuest","BoatQuest",workspace.NPCs:FindFirstChild("Luxury Boat Dealer"))  
								elseif i == "Good" and v == -1 then
									if game:GetService("Workspace").Enemies:FindFirstChild("Cake Queen") then
										for i1,v1 in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
											if v1.Name == "Cake Queen" then
												if v1:FindFirstChild("Humanoid") and v1:FindFirstChild("HumanoidRootPart") and v1.Humanoid.Health > 0 then
													repeat wait()
														AutoHaki()
														EquipWeapon(_G.Settings.Config["Select Weapon"])
														v1.HumanoidRootPart.CanCollide = false
														v1.Humanoid.WalkSpeed = 0
														toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0,25,0))
														if not _G.Settings.Config["Fast Attack"] then
															game:GetService("VirtualUser"):CaptureController()
															game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
														end
														sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius",math.huge)
													until not _G.Settings.Sword["CDK"] or not v1.Parent or v1.Humanoid.Health <= 0
												end
											end
										end
									else
										if game:GetService("ReplicatedStorage"):FindFirstChild("Cake Queen") then
										toTarget(game:GetService("ReplicatedStorage"):FindFirstChild("Cake Queen").HumanoidRootPart.CFrame * CFrame.new(5,10,7))
										else
											wait(3)
											Fluent:Notify({
												Title = "Thunder Notification",
												Content = "Cake Queen",
												SubContent = "Not Spawn Yet !",
												Duration = 3
											})
										end
									end
								elseif i == "Good" and v == -4 then
									for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
										if v.Name == "Raider" or v.Name == "Mercenary" or v.Name == "Swan Pirate" or v.Name == "Factory Staff" or v.Name == "Marine Lieutenant" or v.Name == "Marine Captain" or v.Name == "Vampire" or v.Name == "Zombie" or v.Name == "Snow Trooper" or v.Name == "Winter Warrior" or v.Name == "Lab Subordinate" or v.Name == "Horned Warrior" or v.Name == "Magma Ninja" or v.Name == "Lava Pirate" or v.Name == "Ship Deckhand" or v.Name == "Ship Engineer" or v.Name == "Ship Steward" or v.Name == "Ship Officer" or v.Name == "Arctic Warrior" or v.Name == "Snow Lurker" or v.Name == "Sea Soldier" or v.Name == "Water Fighter" then
											if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
												repeat wait()
													AutoHaki()
													EquipWeapon(_G.Settings.Config["Select Weapon"])
													FastAttack = true
													v.HumanoidRootPart.CanCollide = false
													--v.Humanoid.WalkSpeed = 0
													v.Head.CanCollide = false 
													toTargetP(v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
													--v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
													if not _G.Settings.Config["Fast Attack"] then
														game:GetService("VirtualUser"):CaptureController()
														game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
													end
												until not _G.Settings.Sword["CDK"] or not v.Parent or v.Humanoid.Health <= 0
												FastAttack = false
											end
										else
											if Auto_Farm_Level then _G.Settings.Main["Auto Farm Level"] = false end
											local CastlePos = CFrame.new(-5084.8447265625, 316.48101806641, -3145.3752441406)
											if (CastlePos.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 3000 then
												game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-4987.30908, 314.515503, -3060.17725, 0.990336478, 0, 0.13868615, -0, 1.00000012, -0, -0.138686165, 0, 0.990336359))
											end
											wait(3)
											Fluent:Notify({
												Title = "Tushita Quest",
												Content = "Waiting for Pirate Raid !",
												Duration = 3
											})
										end
									end     
								elseif i == "Evil" and v == -3 then
									repeat wait()
										toTargetP(CFrame.new(56.6141129, 24.7939301, -12861.0859, 0.699276984, 0, 0.714850843, 0, 1, 0, -0.714850843, 0, 0.699276984))
									until not _G.Settings.Sword["CDK"]
								elseif i == "Evil" and v == -4 then
									for i2,v2 in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
										if v2:FindFirstChild("HazeESP") then
											v2.HazeESP.MaxDistance = "inf"
											print("Haze Killing : ",v2.Name)
											repeat wait()
												StartMagnet = true
												FastAttack = true
												-- sethiddenproperty(workspace, "StreamingEnabled", false)
												EquipWeapon(_G.Settings.Config["Select Weapon"])
												AutoHaki()  
												PosMonsEsp = v2.HumanoidRootPart.CFrame 
												toTarget(v2.HumanoidRootPart.CFrame * CFrame.new(0,25,0))
												if not _G.Settings.Config["Fast Attack"] then
													game:GetService("VirtualUser"):CaptureController()
													game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672),workspace.CurrentCamera.CFrame)
												end
											until not _G.Settings.Sword["CDK"] or not v2.Parent or not v2:FindFirstChild("HazeESP") or v2.Humanoid.Health <= 0 
											FastAttack = false
											StartMagnet = false
										else
											for x,y in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
												if y:FindFirstChild("HazeESP") then
													--repeat wait()
														print("Killing : ",y.Name)
														toTarget(y.HumanoidRootPart.CFrame * CFrame.new(0,20,0))
												--	until (y.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 or not _G.Settings.Sword["CDK"]
												end
											end
										end
									end
								elseif i == "Evil" and v == -5 then
									if game:GetService("Workspace").Enemies:FindFirstChild("Soul Reaper") then
										for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
											if string.find(v.Name , "Soul Reaper") then
												repeat wait()
													EquipWeapon(_G.Settings.Config["Select Weapon"])
													AutoHaki()
													toTarget(v.HumanoidRootPart.CFrame*CFrame.new(5,10,7))
													if not _G.Settings.Config["Fast Attack"] then
														game:GetService("VirtualUser"):CaptureController()
														game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 670))
													end
													FastAttack = true
													v.HumanoidRootPart.Transparency = 1
													sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius",math.huge)
												until v.Humanoid.Health <= 0 or _G.Settings.Sword["Scythe"] == false
												FastAttack = false
											end
										end
									elseif game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Hallow Essence") or game:GetService("Players").LocalPlayer.Character:FindFirstChild("Hallow Essence") then
										repeat toTargetP(CFrame.new(-8932.322265625, 146.83154296875, 6062.55078125)) wait() until (CFrame.new(-8932.322265625, 146.83154296875, 6062.55078125).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 8                        
										EquipWeapon("Hallow Essence")
									else
										if game:GetService("ReplicatedStorage"):FindFirstChild("Soul Reaper") then
											toTargetP(game:GetService("ReplicatedStorage"):FindFirstChild("Soul Reaper").HumanoidRootPart.CFrame * CFrame.new(5,10,7))
										else
											Fluent:Notify({
												Title = "Thunder Notification",
												Content = "Cursed Dual Katana",
												SubContent = "Soul Reaper Not Spawn Yet !",
												Duration = 3
											})
											wait(3)
										end
									end
								end
							end
						else
							Fluent:Notify({
								Title = "Thunder Notification",
								Content = "Cursed Dual Katana",
								SubContent = "You already CDK on Your Inventory !",
								Duration = 3
							})
							wait(3)
						end
					end
				end)
			end
		end)

		local SoulGuitar = Tabs.ItemFunction:AddSection("[ Soul Guitar ]")
		SoulGuitar:AddToggle("Soul", {Title = "Auto Soul Guitar", Default = _G.Settings.Sword["Soul Guitar"]})
		Options.Soul:OnChanged(function()
			_G.Settings.Sword["Soul Guitar"] = Options["Soul"].Value
			if Options["Soul"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)

		spawn(function()
			while wait() do
				pcall(function()
					if _G.Settings.Sword["Soul Guitar"] and World3 then
						if GetWeaponInventory("Soul Guitar") == false then
							if (CFrame.new(-9681.458984375, 6.139880657196045, 6341.3720703125).Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 5000 then
								if string.find(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("gravestoneEvent",2), "Error") then
									toTargetP(CFrame.new(-8653.2060546875, 140.98487854003906, 6160.033203125))
								elseif string.find(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("gravestoneEvent",2), "Nothing") then
								else
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("gravestoneEvent",2,true)
								end
								for i,v in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("GuitarPuzzleProgress","Check")) do
									if i == "Swamp" and v == false then
										if game:GetService("Workspace").Enemies:FindFirstChild("Living Zombie") then
											for i1,v1 in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
												if v1.Name == "Living Zombie" then
													if v1:FindFirstChild("Humanoid") and v1:FindFirstChild("HumanoidRootPart") and v1.Humanoid.Health > 0 then
														repeat wait()
															AutoHaki()
															EquipWeapon(_G.Settings.Config["Select Weapon"])
															v1.HumanoidRootPart.CanCollide = false
															v1.Humanoid.WalkSpeed = 0
															v1.Head.CanCollide = false 
															StartMagnet = true
															FastAttack = true
															PosMon = v1.HumanoidRootPart.CFrame
															v1.HumanoidRootPart.CFrame = CFrame.new(-10185, 138, 6011)
															toTargetP(v1.HumanoidRootPart.CFrame * CFrame.new(0,25,0))
															if not _G.Settings.Config["Fast Attack"] then
																game:GetService("VirtualUser"):CaptureController()
																game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
															end
														until not _G.Settings.Sword["Soul Guitar"] or not v1.Parent or v1.Humanoid.Health <= 0
													end
												end
											end
										else
											StartMagnet = false
											FastAttack = false
											for i2,v2 in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do 
												if v2.Name == "Living Zombie" then
													toTargetP(v2.HumanoidRootPart.CFrame * CFrame.new(0,25,0))
												end
											end
											toTargetP(CFrame.new(-10185.2676, 138.652512, 6011.1499, 0.259815037, 0, 0.965658426, 0, 1, 0, -0.965658426, 0, 0.259815037))
										end
									elseif i == "Swamp" and v == true then
										local nomor = math.random(1,6)
										local Kanan = game:GetService("Workspace").Map["Haunted Castle"]["Placard"..nomor]["Right"]
										local Kiri = game:GetService("Workspace").Map["Haunted Castle"]["Placard"..nomor]["Right"]
										for o,p in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("GuitarPuzzleProgress","Check")) do 
											if o == "Gravestones" and p == false then
												for i1,v1 in pairs(Kanan:GetChildren()) do
													if v1.Name == "ClickDetector" then
														fireclickdetector(Kiri.ClickDetector)
														wait(.1)
														fireclickdetector(Kanan.ClickDetector)
														wait()
														fireclickdetector(Kanan.ClickDetector)
														wait(.1)
														fireclickdetector(Kiri.ClickDetector)
														wait(.1)
														fireclickdetector(Kiri.ClickDetector)
													end
												end
											end
										end
									elseif i == "Gravestones" and v == true then
										for o,p in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("GuitarPuzzleProgress","Check")) do 
											if o == "Ghost" and p == false then
												game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("GuitarPuzzleProgress","Ghost")
											end
										end
									elseif i == "Ghost" and v == true then
										for o,p in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("GuitarPuzzleProgress","Check")) do 
											if o == "Trophies" and p == false then
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment1.ClickDetector)
												wait(.1)
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment2.ClickDetector)
												wait(.1)
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment3.ClickDetector)
												wait(.1)
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment4.ClickDetector)
												wait(.1)
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment5.ClickDetector)
												wait(.1)
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment6.ClickDetector)
												wait(.1)
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment7.ClickDetector)
												wait(.1)
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment8.ClickDetector)
												wait(.1)
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment9.ClickDetector)
												wait(.1)
												fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"].Tablet.Segment10.ClickDetector)
												wait(.1)
											end
										end
									elseif i == "Trophies" and v == true then
										for o,p in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("GuitarPuzzleProgress","Check")) do 
											if o == "Trophies" and p == false then
												if game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part1:FindFirstChild("ClickDetector") then
													Quest4 = true
													repeat wait() 
														toTargetP(CFrame.new(-9553.5986328125, 65.62338256835938, 6041.58837890625)) 
													until (CFrame.new(-9553.5986328125, 65.62338256835938, 6041.58837890625).Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 3 or not _G.QuestSoulGuitar
													wait(1)
													toTargetP(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part3.CFrame)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part3.ClickDetector)
													wait(1)
													toTargetP(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part4.CFrame)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part4.ClickDetector)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part4.ClickDetector)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part4.ClickDetector)
													wait(1)
													toTargetP(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part6.CFrame)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part6.ClickDetector)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part6.ClickDetector)
													wait(1)
													toTargetP(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part8.CFrame)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part8.ClickDetector)
													wait(1)
													totoTargetPpos(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part10.CFrame)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part10.ClickDetector)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part10.ClickDetector)
													wait(1)
													fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Lab Puzzle"].ColorFloor.Model.Part10.ClickDetector)
												end
											end
										end
									end
								end
							else
								toTarget(CFrame.new(-9681.458984375, 6.139880657196045, 6341.3720703125))
							end
						else
							Fluent:Notify({
								Title = "Thunder Notification",
								Content = "Soul Guitar",
								SubContent = "You already Have Soul Guitar on Your Inventory !",
								Duration = 3
							})
							wait(3)
						end
					end
				end)
			end
		end)

		local AnchorSection = Tabs.ItemFunction:AddSection("[ Shark Anchor ]")
		AnchorSection:AddToggle("Srot", {Title = "Auto Shark Anchor Full", Default = _G.Settings.Sword["Sea Anchor"]})
		Options.Srot:OnChanged(function()
			_G.Settings.Sword["Sea Anchor"] = Options["Srot"].Value
			if Options["Srot"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)

		spawn(function()
			while wait() do
				if _G.Settings.Sword["Sea Anchor"] and World3 then
					pcall(function()
						if GetWeaponInventory("Shark Anchor") == false then
							if GetWear("Shark Tooth Necklace") == false then
								if GetMaterial("Shark Tooth") >= 5 and GetMaterial("Mutant Tooth") >= 1 then
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CraftItem","Check","ToothNecklace")
								elseif not GetMaterial("Shark Tooth") or GetMaterial("Shark Tooth") <= 4 then
									local enemyTypes = {
										{name = "Terrorshark", variable = "bjirTerrorshark"},
										{name = "Shark", variable = "bjirShark"},
										{name = "Piranha", variable = "bjirPiranha"},
										{name = "FishBoat", variable = "bjirFishBoatddd"},
									}
									for _, enemyType in pairs(enemyTypes) do
										local enemyName = enemyType.name
										if game:GetService("Workspace").Enemies:FindFirstChild(enemyName) then
											for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
												if v.Name == enemyName then
													if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
														repeat wait()
															game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = false
															Mutant = true
															EquipWeapon(_G.Settings.Config["Select Weapon"]);
															AutoHaki()
															v.HumanoidRootPart.CanCollide = false
															PosMon = v.HumanoidRootPart.CFrame
															toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
															if not _G.Settings.Config["Fast Attack"] then
																game:GetService("VirtualUser"):CaptureController()
																game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
															end
															FastAttack = true
														until not _G.Settings.Sword["Sea Anchor"] or not v.Parent or v.Humanoid.Health <= 0 or GetMaterial("Shark Tooth") >= 1
														FastAttack = false
														Mutant = false
													end
												end
											end
										else
											if not game:GetService("Workspace").Boats:FindFirstChild(_G.Settings.Main["Select Boats"]) then
												buyb = toTargetP(CFrame.new(-16228, 9, 480))
												if (CFrame.new(-16228, 9, 480).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
													if buyb then buyb:Stop() end wait(.1)
													local args = {
														[1] = "BuyBoat",
														[2] = _G.Settings.Main["Select Boats"]
													}
													game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
												end
											else
												if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
													toTargetP(game:GetService("Workspace").Boats:FindFirstChild(_G.Settings.Main["Select Boats"]).VehicleSeat.CFrame * CFrame.new(0,1,0))
												else
													for i,v in pairs(game:GetService("Workspace").Boats:GetChildren()) do
														if v.Name == (_G.Settings.Main["Select Boats"]) then
															repeat wait()
																dangerzone = ShipTP(CFrame.new(-30939.830078125, 3.729933261871338, 9256.4208984375))
																for _, t in pairs(workspace.Boats:FindFirstChild(_G.Settings.Main["Select Boats"]):GetDescendants()) do
																	if t:IsA("BasePart") or v:IsA("Part") or t.ClassName == "MeshPart" then
																		if (CFrame.new(-30939.830078125, 3.729933261871338, 9256.4208984375).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude >= 10 then
																			t.CanCollide = false   
																		else
																			t.CanCollide = true
																			if dangerzone then dangerzone:Stop() end wait(.1)
																		end
																	end
																end
															until Shark == true or _G.Settings.Sword["Sea Anchor"] == false or game:GetService("Workspace").Enemies:FindFirstChild(enemyName)
														end
													end
												end
											end
										end
									end
								elseif not GetMaterial("Mutant Tooth") or GetMaterial("Mutant Tooth") <= 0 then
									local enemyTypes = {
										{name = "Terrorshark", variable = "bjirTerrorshark"},
										{name = "Shark", variable = "bjirShark"},
										{name = "Piranha", variable = "bjirPiranha"},
										{name = "FishBoat", variable = "bjirFishBoatddd"},
									}
									for _, enemyType in pairs(enemyTypes) do
										local enemyName = enemyType.name
										if game:GetService("Workspace").Enemies:FindFirstChild(enemyName) then
											for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
												if v.Name == enemyName then
													if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
														repeat wait()
															game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = false
															Mutant = true
															EquipWeapon(_G.Settings.Config["Select Weapon"]);
															AutoHaki()
															v.HumanoidRootPart.CanCollide = false
															PosMon = v.HumanoidRootPart.CFrame
															toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
															if not _G.Settings.Config["Fast Attack"] then
																game:GetService("VirtualUser"):CaptureController()
																game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
															end
															FastAttack = true
														until not _G.Settings.Sword["Sea Anchor"] or not v.Parent or v.Humanoid.Health <= 0 or GetMaterial("Mutant Tooth") >= 1
														FastAttack = false
														Mutant = false
													end
												end
											end
										else
											if not game:GetService("Workspace").Boats:FindFirstChild(_G.Settings.Main["Select Boats"]) then
												buyb = toTargetP(CFrame.new(-30939.830078125, 3.729933261871338, 9256.4208984375))
												if (CFrame.new(-30939.830078125, 3.729933261871338, 9256.4208984375).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
													if buyb then buyb:Stop() end wait(.1)
													local args = {
														[1] = "BuyBoat",
														[2] = _G.Settings.Main["Select Boats"]
													}
													game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
												end
											elseif game:GetService("Workspace").Boats:FindFirstChild(_G.Settings.Main["Select Boats"]) then
												if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
													toTargetP(game:GetService("Workspace").Boats:FindFirstChild(_G.Settings.Main["Select Boats"]).VehicleSeat.CFrame * CFrame.new(0,1,0))
												else
													for i,v in pairs(game:GetService("Workspace").Boats:GetChildren()) do
														if v.Name == (_G.Settings.Main["Select Boats"]) then
															repeat wait()
																dangerzone = ShipTP(CFrame.new(-30939.830078125, 3.729933261871338, 9256.4208984375))
																for _, t in pairs(workspace.Boats:FindFirstChild(_G.Settings.Main["Select Boats"]):GetDescendants()) do
																	if t:IsA("BasePart") or v:IsA("Part") or t.ClassName == "MeshPart" then
																		if (CFrame.new(-30939.830078125, 3.729933261871338, 9256.4208984375).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude >= 10 then
																			t.CanCollide = false   
																		else
																			t.CanCollide = true
																			if dangerzone then dangerzone:Stop() end wait(.1)
																		end
																	end
																end
															until Mutant == true or _G.Settings.Sword["Sea Anchor"] == false or game:GetService("Workspace").Enemies:FindFirstChild("Terrorshark")
														end
													end
												end
											end
										end
									end
								end
							elseif GetWear("Terror Jaw") == false then
								game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CraftItem","Check","TerrorJaw")
								print("Crafting Terror Jaw")
							elseif GetMaterial("Monster Magnet") == false then
								game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CraftItem","PossibleHardcode","SharkAnchor")
							elseif GetMaterial("Monster Magnet") == true and GetWear("Terror Jaw") == true and GetWear("Shark Tooth Necklace") == true then
								--Killing Terrorshark
							end
						else
							wait(3)
							Fluent:Notify({
								Title = "Thunder Notification",
								Content = "Shark Anchor",
								SubContent = "You already Have Shark Anchor on Your Inventory !",
								Duration = 3
							})
						end
					end)
				end
			end
		end)

		local YamaSection = Tabs.ItemFunction:AddSection("[ Yama ]")
		YamaSection:AddToggle("Yama", {Title = "Auto Yama", Default = _G.Settings.Sword["Yama"]})
		Options.Yama:OnChanged(function()
			_G.Settings.Sword["Yama"] = Options["Yama"].Value
			if Options["Yama"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
		end)
		spawn(function()
			while wait() do
				if _G.Settings.Sword["Yama"] and World3 then
					if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EliteHunter","Progress") >= 30 and GetWeaponInventory("Yama") == false then
						repeat wait()
							fireclickdetector(game:GetService("Workspace").Map.Waterfall.SealedKatana.Handle.ClickDetector)
						until game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Yama") or not _G.Settings.Sword["Yama"] or GetWeaponInventory("Yama") == true
					else
						if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EliteHunter","Progress") <= 30 then
							Fluent:Notify({
								Title = "Thunder Notification",
								Content = "Yama",
								SubContent = "Your Elite progress is less 30 !",
								Duration = 3
							})
							wait(3)
						elseif GetWeaponInventory("Yama") == true then
							Fluent:Notify({
								Title = "Thunder Notification",
								Content = "Yama",
								SubContent = "You already Yama on Your Inventory !",
								Duration = 3
							})
							wait(3)
						end
					end
				end
			end
		end)

		local TushitaSection = Tabs.ItemFunction:AddSection("[ Tushita ]")
		TushitaSection:AddToggle("Tushita", {Title = "Auto Tushita", Default = _G.Settings.Sword["Tushita"]})
		Options.Tushita:OnChanged(function()
			_G.Settings.Sword["Tushita"] = Options["Tushita"].Value
			if Options["Tushita"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)

		TushitaSection:AddToggle("TushitaHop", {Title = "Auto Tushita Hop", Default = _G.Settings.Sword["Tushita Hop"]})
		Options.TushitaHop:OnChanged(function()
			_G.Settings.Sword["Tushita Hop"] = Options["TushitaHop"].Value
			SaveSettings()
		end)
		
		spawn(function()
			while wait() do
				if _G.Settings.Sword["Tushita"] and World3 and game.Players.LocalPlayer.Data.Level.Value >= 2000 then
					pcall(function()
						if GetWeaponInventory("Tushita") == false then
							if Auto_Farm_Level then _G.Settings.Main["Auto Farm Level"] = false end
							if game:GetService("Workspace").Map.Turtle:FindFirstChild("TushitaGate").TushitaGate.Transparency == 0 then
								--for i,v in pairs (game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TushitaProgress")) do
									if game:GetService("Workspace").Map.Waterfall.SecretRoom.Room.Door.Door.Hitbox.Transparency == 0 then
										if not game:GetService("Workspace").Enemies:FindFirstChild("rip_indra True Form") and not game.ReplicatedStorage:FindFirstChild("rip_indra True Form") then
											if _G.Settings.Sword["Tushita Hop"] then
												wait(1)
												Hop()
											end
										end
									else
										if game:GetService("Workspace").Enemies:FindFirstChild("rip_indra True Form") and game.ReplicatedStorage:FindFirstChild("rip_indra True Form") then
											if not (game.Players.LocalPlayer.Backpack:FindFirstChild("Holy Torch") or game.Players.LocalPlayer.Character:FindFirstChild("Holy Torch")) then
												toTarget(5148, 162, 910)
											else
												repeat toTarget(CFrame.new(-10752, 417, -9366)) wait() until not _G.Settings.Sword["Tushita"] or (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-Vector3.new(-10752, 417, -9366)).Magnitude <= 10
												wait(1)
												repeat toTarget(CFrame.new(-11672, 334, -9474)) wait() until not _G.Settings.Sword["Tushita"] or (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-Vector3.new(-11672, 334, -9474)).Magnitude <= 10
												wait(1)
												repeat toTarget(CFrame.new(-12132, 521, -10655)) wait() until not _G.Settings.Sword["Tushita"] or (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-Vector3.new(-12132, 521, -10655)).Magnitude <= 10
												wait(1)
												repeat toTarget(CFrame.new(-13336, 486, -6985)) wait() until not _G.Settings.Sword["Tushita"] or (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-Vector3.new(-13336, 486, -6985)).Magnitude <= 10
												wait(1)
												repeat toTarget(CFrame.new(-13489, 332, -7925)) wait() until not _G.Settings.Sword["Tushita"] or (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-Vector3.new(-13489, 332, -7925)).Magnitude <= 10
											end
										end
									end
								--end
							else
								if game:GetService("Workspace").Enemies:FindFirstChild("Longma") then
									for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
										if v.Name == ("Longma" or v.Name == "Longma") and v.Humanoid.Health > 0 and v:IsA("Model") and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") then
											repeat wait()
												FastAttack = true
												AutoHaki()
												EquipWeapon(_G.Settings.Config["Select Weapon"])
												if not _G.Settings.Config["Fast Attack"] then
													game:GetService'VirtualUser':CaptureController()
													game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
												end
												toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
											until not _G.Settings.Sword["Tushita"] or not v.Parent or v.Humanoid.Health <= 0
											FastAttack = false
										end
									end
								else
									if not game.ReplicatedStorage:FindFirstChild("Longma") then
										wait(3)
										Fluent:Notify({
											Title = "Boss Status",
											Content = "Longma",
											SubContent = "Not Spawn Yet !",
											Duration = 3
										})
									else
										toTarget(CFrame.new(-10238.875976563, 389.7912902832, -9549.7939453125))
									end
								end
							end
						else
							wait(3)
							Fluent:Notify({
								Title = "Sword Status",
								Content = "Tuhsita",
								SubContent = "Already Have Tushita !",
								Duration = 3
							})
						end
					end)
				end
			end
		end)

		local HollowScythe = Tabs.ItemFunction:AddSection("[ Hallow Scythe ]")
		HollowScythe:AddToggle("Hollow", {Title = "Auto Hollow Scythe", Default = _G.Settings.Sword["Scythe"]})
		Options.Hollow:OnChanged(function()
			_G.Settings.Sword["Scythe"] = Options["Hollow"].Value
			if Options["Hollow"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)
		HollowScythe:AddToggle("Scythe", {Title = "Auto Hallow Scythe Hop", Default = _G.Settings.Sword["Scythe Hop"]})
		Options.Scythe:OnChanged(function()
			_G.Settings.Sword["Scythe Hop"] = Options["Scythe"].Value
			SaveSettings()
		end)

		spawn(function()
			while wait() do
				if _G.Settings.Sword["Scythe"] then
					pcall(function()
						if GetWeaponInventory("Hallow Scythe") == false then
							if game:GetService("Workspace").Enemies:FindFirstChild("Soul Reaper") then
								for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
									if string.find(v.Name , "Soul Reaper") then
										repeat wait()
											EquipWeapon(_G.Settings.Config["Select Weapon"])
											AutoHaki()
											toTarget(v.HumanoidRootPart.CFrame*CFrame.new(5,10,7))
											if not _G.Settings.Config["Fast Attack"] then
												game:GetService("VirtualUser"):CaptureController()
												game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 670))
											end
											FastAttack = true
											v.HumanoidRootPart.Transparency = 1
											sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius",math.huge)
										until v.Humanoid.Health <= 0 or _G.Settings.Sword["Scythe"] == false
										FastAttack = false
									end
								end
							elseif game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Hallow Essence") or game:GetService("Players").LocalPlayer.Character:FindFirstChild("Hallow Essence") then
								if Auto_Farm_Level then
									_G.Settings.Main["Auto Farm Level"] = false
								end
								if _G.Settings.Main["Auto Farm Bone"] then
									_G.Settings.Main["Auto Farm Bone"] = false
								end
								repeat toTarget(CFrame.new(-8932.322265625, 146.83154296875, 6062.55078125)) wait() until (CFrame.new(-8932.322265625, 146.83154296875, 6062.55078125).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 8                        
								EquipWeapon("Hallow Essence")
							else
								if game:GetService("ReplicatedStorage"):FindFirstChild("Soul Reaper") then
									toTarget(game:GetService("ReplicatedStorage"):FindFirstChild("Soul Reaper").HumanoidRootPart.CFrame * CFrame.new(5,10,7))
								else
									if _G.Settings.Sword["Scythe Hop"] then
										Hop()
									end
								end
							end
						else
							Fluent:Notify({
								Title = "Thunder Notification",
								Content = "Hallow Scythe",
								SubContent = "You already Hallow Scythe on Your Inventory !",
								Duration = 3
							})
							wait(3)
						end
					end)
				end
			end
		end)
		
		

	else
		_G.Settings.Sword["CDK"] = false
		_G.Settings.Sword["Yama"] = false
		_G.Settings.Sword["Tushita"] = false
		_G.Settings.Sword["Tushita Hop"] = false
		_G.Settings.Sword["Scythe"] = false
		_G.Settings.Sword["Scythe Hop"] = false
	end

	if World2 then
		local RengokuSection = Tabs.ItemFunction:AddSection("[ Rengoku ]")
		RengokuSection:AddToggle("Rengoku", {Title = "Auto Rengoku", Default = _G.Settings.Sword["Rengoku"]})
		Options.Rengoku:OnChanged(function()
			_G.Settings.Sword["Rengoku"] = Options["Rengoku"].Value
			if Options["Rengoku"].Value == false then
				wait()
				toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
			end
			SaveSettings()
		end)

		spawn(function()
			pcall(function()
				if _G.Settings.Sword["Rengoku"] then
					while wait() do
						if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Hidden Key") or game:GetService("Players").LocalPlayer.Character:FindFirstChild("Hidden Key") then
							EquipWeapon("Hidden Key")
							toTargetP(CFrame.new(6571.1201171875, 299.23028564453, -6967.841796875))
						elseif game:GetService("Workspace").Enemies:FindFirstChild("Snow Lurker") or game:GetService("Workspace").Enemies:FindFirstChild("Arctic Warrior") then
							for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
								if (v.Name == "Snow Lurker" or v.Name == "Arctic Warrior") and v.Humanoid.Health > 0 then
									repeat wait()
										EquipWeapon(_G.Settings.Config["Select Weapon"]);
										AutoHaki()
										v.HumanoidRootPart.CanCollide = false
										PosMon = v.HumanoidRootPart.CFrame
										toTarget(v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
										if not _G.Settings.Config["Fast Attack"] then
											game:GetService("VirtualUser"):CaptureController()
											game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
										end
										StartMagnet = true
										FastAttack = true
									until game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Hidden Key") or game:GetService("Players").LocalPlayer.Character:FindFirstChild("Hidden Key") or _G.Settings.Sword["Rengoku"] == false or not v.Parent or v.Humanoid.Health <= 0
									StartMagnet = false
									FastAttack = false
								end
							end
						else
							StartMagnet = false
							FastAttack = false
							toTarget(CFrame.new(5439.716796875, 84.420944213867, -6715.1635742188))
						end
					end
				end
			end)
		end)
	else
		_G.Settings.Sword["Rengoku"] = false
	end
	
