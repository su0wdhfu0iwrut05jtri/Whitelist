			_G.Settings.Main["Random Suprise"] = Options["RandomSuprise"].Value
			SaveSettings()
		end)

		spawn(function()
			while wait() do
				if _G.Settings.Main["Random Suprise"] then
					if not game.Players.LocalPlayer.Backpack:FindFirstChild("Hallow Essence") or not game.Players.LocalPlayer.Character:FindFirstChild("Hallow Essence") then
