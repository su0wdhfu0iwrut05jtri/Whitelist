			SaveSettings()
		end)

		spawn(function()
			while wait() do
				if _G.Settings.Main["Pirate Raid"] and World3 then
					pcall(function()
						for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
							if v.Name == "Raider" or v.Name == "Mercenary" or v.Name == "Swan Pirate" or v.Name == "Factory Staff" or v.Name == "Marine Lieutenant" or v.Name == "Marine Captain" or v.Name == "Vampire" or v.Name == "Zombie" or v.Name == "Snow Trooper" or v.Name == "Winter Warrior" or v.Name == "Lab Subordinate" or v.Name == "Horned Warrior" or v.Name == "Magma Ninja" or v.Name == "Lava Pirate" or v.Name == "Ship Deckhand" or v.Name == "Ship Engineer" or v.Name == "Ship Steward" or v.Name == "Ship Officer" or v.Name == "Arctic Warrior" or v.Name == "Snow Lurker" or v.Name == "Sea Soldier" or v.Name == "Water Fighter" then
								if Auto_Farm_Level then _G.Settings.Main["Auto Farm Level"] = false end
								if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
									repeat wait()
										AutoHaki()
										EquipWeapon(_G.Settings.Config["Select Weapon"])
										FastAttack = true
										v.HumanoidRootPart.CanCollide = false
										--v.Humanoid.WalkSpeed = 0
										v.Head.CanCollide = false 
										toTargetP(v.HumanoidRootPart.CFrame * CFrame.new(0,_G.Settings.Config["Distance Enemy"],0))
										--v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
										if not _G.Settings.Config["Fast Attack"] then
											game:GetService("VirtualUser"):CaptureController()
											game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
										end
									until not _G.Settings.Main["Pirate Raid"] or not v.Parent or v.Humanoid.Health <= 0
									FastAttack = false
								end
							else
								for i,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
									if v.Name == "Raider" or v.Name == "Mercenary" or v.Name == "Swan Pirate" or v.Name == "Factory Staff" or v.Name == "Marine Lieutenant" or v.Name == "Marine Captain" or v.Name == "Vampire" or v.Name == "Zombie" or v.Name == "Snow Trooper" or v.Name == "Winter Warrior" or v.Name == "Lab Subordinate" or v.Name == "Horned Warrior" or v.Name == "Magma Ninja" or v.Name == "Lava Pirate" or v.Name == "Ship Deckhand" or v.Name == "Ship Engineer" or v.Name == "Ship Steward" or v.Name == "Ship Officer" or v.Name == "Arctic Warrior" or v.Name == "Snow Lurker" or v.Name == "Sea Soldier" or v.Name == "Water Fighter" then
										if Auto_Farm_Level then _G.Settings.Main["Auto Farm Level"] = false end
										local CastlePos = CFrame.new(-5084.8447265625, 316.48101806641, -3145.3752441406)
										if (CastlePos.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 3000 then
											game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-4987.30908, 314.515503, -3060.17725, 0.990336478, 0, 0.13868615, -0, 1.00000012, -0, -0.138686165, 0, 0.990336359))
										end
									end
								end
							end
						end                                                        
					end)
				end
			end
		end)

