							v:Destroy() 
						end
					end
					for i,v in pairs(game:GetService("ReplicatedStorage").Effect.Container.Hit:GetChildren()) do
						if v.Name == "Weak" or v.Name == "Clash" or v.Name == "Basic" or v.Name == "Combat" then
