				if _G.Settings.Main["Auto Color Haki Dealer"] then
					pcall(function()
						for i,v in pairs(_G.Settings.Main["Color List"]) do
							if i == string.gsub(game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("ColorsDealer", "1"), "%d+", "") then
								game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ColorsDealer","2")
							end
							if _G.Settings.Main["Auto Color Haki Dealer Hop"] then
								wait(5)
								Hop()
							end
						end
					end)
				end
			end
		end)

