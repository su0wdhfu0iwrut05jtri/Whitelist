            Multi = true,
            Default = _G.Settings.Stats["Dropdown Stats"],
        })
        
        Options.StatsDropdown:OnChanged(function()
            _G.Settings.Stats["Dropdown Stats"] = Options["StatsDropdown"].Value
            SaveSettings()
        end)
        
        spawn(function()
            while wait() do
