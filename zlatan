        end)

        Island:AddDropdown("Modek", {
            Title = "Select Mode Teleport",
            Values = {"Tween","Instant"},
            Multi = false,
            Default = _G.Settings.Main["Mode TP"],
        })
        
        Options.Modek:OnChanged(function()
            _G.Settings.Main["Mode TP"] = Options["Modek"].Value
        end)

        Island:AddToggle("IslandToggle", {Title = "Teleport to Island", Default = _G.Settings.Main["Island Toggle"]})
        Options.IslandToggle:OnChanged(function()
            _G.Settings.Main["Island Toggle"] = Options["IslandToggle"].Value
            if Options["IslandToggle"].Value == false then
                wait()
                toTargetP(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
                wait()
            end
        end)

        spawn(function()
            while wait() do
                if _G.Settings.Main["Island Toggle"] then
                    pcall(function()
                        if _G.Settings.Main["Island"] == "Starter Island Lv 1" then
                            if _G.Settings.Main["Mode TP"] == "Tween" then
                                toTargetP(CFrame.new(-2146, 44, -3200))
