                            if (CFrame.new(-2117, 45, -2942).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 100 then
                                if game.Players.LocalPlayer.Backpack:FindFirstChild("Poneglyph Scroll") then
                                    local tool = game.Players.LocalPlayer.Backpack:FindFirstChild("Poneglyph Scroll")
                                    wait(.4)
                                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
