    game:GetService("Players").LocalPlayer.Backpack.ChildAdded:Connect(
        function(Wapon)
            local weaponName = Wapon.Name
            if Wapon:IsA("Tool") then
                table.insert(WeaponList, weaponName)
            end
            Options.WeaponSelector:SetValues()
        end
    )
    game:GetService("Players").LocalPlayer.Backpack.ChildRemoved:Connect(
        function(Wapon)
            local weaponName = Wapon.Name
            if Wapon:IsA("Tool") then
                for i,v in pairs(WeaponList) do
                    if v == weaponName then
                        table.remove(WeaponList, i)
                    end
                end
            end
            Options.WeaponSelector:SetValues()
        end
    )
    
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        UpdateWeapon()
    end)
    
    function UpdateWeapon()
        game:GetService("Players").LocalPlayer.Backpack.ChildAdded:Connect(
            function(Wapon)
                local weaponName = Wapon.Name
                if Wapon:IsA("Tool") then
                    table.insert(WeaponList, weaponName)
                end
                Options.WeaponSelector:SetValues()
            end
        )
        game:GetService("Players").LocalPlayer.Backpack.ChildRemoved:Connect(
            function(Wapon)
                local weaponName = Wapon.Name
                if Wapon:IsA("Tool") then
                    for i,v in pairs(WeaponList) do
                        if v == weaponName then
                            table.remove(WeaponList, i)
                        end
                    end
                end
                Options.WeaponSelector:SetValues()
            end
        )
    end

