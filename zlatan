        BossTable = {"[Lv 25] Bandit Boss", "[Lv 60] Clown Boss", "[Lv 120] Shark Boss", "[Lv 200] Bomb Boss", "[Lv 300] Krieg Boss", "[Lv 400] Tashii", "[Lv 550] King Gorilla", "[Lv 650] Marine Captain", "[Lv 750] Minotaur", "[Lv 850] Vice Admiral" ,"[Lv 900] Ice Admiral", "[Lv 1100] Thunder God", "[Lv 1250] Revolutionary Boss", "[Lv 1400] Warden", "[Lv 1500] Vergo", "[Lv 1550] Snow Harpy", "[Lv 1700] Neptune", "[Lv 1850] Shiryu", "[Lv 2000] G4 Boss","[Lv 2150] Ryummy"}
