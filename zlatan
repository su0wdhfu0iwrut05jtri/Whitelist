    spawn(function()
        while task.wait() do
            if _G.Settings.Main["Auto Farm Boss"] then
                pcall(function()
                    CheckBoss()
                    if game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.MainFrame.Frame.Main.List.QuestFrame.Progress.Progress.Text == "0/0" then
                        game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.QuestFunction:InvokeServer(workspace.Npc_Workspace.QuestGivers[NPCQuest], LevelQuest)
                        Questtween = toTargetP(CFrameQuest.Position,CFrameQuest)
                        if (CFrameQuest.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 150 then
                            if Questtween then Questtween:Stop() end
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQuest
                            game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.QuestFunction:InvokeServer(workspace.Npc_Workspace.QuestGivers[NPCQuest], LevelQuest)
                        end
                    else
                        CheckBoss()
                        for i,v in pairs(game:GetService("Workspace")["NPC Zones"][IslandMon].NPCS:GetChildren()) do
                            if string.find(v.Name,NameMon) then
                                if v:FindFirstChild("NPCName").Value == NameMon then
                                    repeat task.wait()
                                        if _G.Settings.Config["Farm Mode"] == "Under" then
                                            FarmTween = toTargetP(v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                        elseif _G.Settings.Config["Farm Mode"] == "Above" then
                                            FarmTween = toTargetP(v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                        elseif  _G.Settings.Config["Farm Mode"] == "Behind" then 
                                            FarmTween = toTargetP(v.HumanoidRootPart.CFrame * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                        end
                                        if _G.Settings.Config["Auto Transform Gear"] then
                                            spawn(function()
                                                pcall(function()
                                                    if _G.Settings.Config["Gear List"] == "Gear 4" then
                                                        if not game:GetService("Workspace")[game.Players.LocalPlayer.Name]:FindFirstChild("Gear4IsOn") then
                                                            game:service('VirtualInputManager'):SendKeyEvent(true, "N", false, game)
                                                            wait(.1)
                                                            game:service('VirtualInputManager'):SendKeyEvent(false, "N", false, game)
                                                        end
                                                    elseif _G.Settings.Config["Gear List"] == "Gear 5" then
                                                        if not game:GetService("Workspace")[game.Players.LocalPlayer.Name]:FindFirstChild("Zoan") then
                                                            game:service('VirtualInputManager'):SendKeyEvent(true, "P", false, game)
