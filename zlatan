        local Island = Tabs.Teleport:AddSection("[ Island ]")
        if World1 then
            IslandList = {"Starter Island Lv 1","Clown Island Lv 40","Shark Park Lv 90","Desert Lv 160","Restaurant Lv 250","Logue Town Lv 350","Tall Woods Lv 450","Marine Town Lv 600","Three Island Lv 700","Marine HQ Lv 800","Sky Island Lv 950","Revolutionary Lv 1150","Impel Jail Lv 1300","Punkhazard Lv 1450","Fishman Island Lv 1600","Skull Island Lv 1750","Bubble Island Lv 1900","Thriller Boat Lv 2050"}
        elseif World2 then
            IslandList = {"Wano Island Lv 2200","Udon Jail Lv 2400","Snowy Graveyard Lv 2550","Desert City Lv 2700","Dragon Island Lv 2850","Shadow Island Lv 3000","Peanut Island Lv 3150","Dough Island Lv 3300","Cake Island Lv 3450"}
