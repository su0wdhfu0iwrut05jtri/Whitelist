    local Ireng = loadstring(game:HttpGet(('https://raw.githubusercontent.com/ThundarZ/Welcome/main/Main/Function/Jawir.lua')))()
    
    spawn(function()
        while task.wait() do
            if _G.Settings.Main["Auto Farm Level"] then
                pcall(function()
                    CheckLevel()
                    local MyLevel = game:GetService("Players").LocalPlayer.PlayerData.Experience.Level.Value
                   if game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.MainFrame.Frame.Main.List.QuestFrame.Progress.Progress.Text == "0/0" then
                        game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.QuestFunction:InvokeServer(workspace.Npc_Workspace.QuestGivers[NPCQuest], LevelQuest)
                    else
                        CheckLevel()
                        for i,v in pairs(game:GetService("Workspace")["NPC Zones"][IslandMon].NPCS:GetChildren()) do
                            if string.find(v.Name,NameMon) then
                                if v:FindFirstChild("NPCName").Value == NameMon then
                                    if v:FindFirstChild("HumanoidRootPart") then
                                        repeat task.wait()
                                            if _G.Settings.Config["Farm Mode"] == "Under" then
                                                FarmTween = toTargetP(v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                            elseif _G.Settings.Config["Farm Mode"] == "Above" then
                                                FarmTween = toTargetP(v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                            elseif  _G.Settings.Config["Farm Mode"] == "Behind" then 
                                                FarmTween = toTargetP(v.HumanoidRootPart.CFrame * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                            end
                                            if _G.Settings.Config["Auto Transform Gear"] then
                                                spawn(function()
                                                    pcall(function()
                                                        if _G.Settings.Config["Gear List"] == "Gear 4" then
                                                            if not game:GetService("Workspace")[game.Players.LocalPlayer.Name]:FindFirstChild("Gear4IsOn") then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "N", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "N", false, game)
                                                            end
                                                        elseif _G.Settings.Config["Gear List"] == "Gear 5" then
                                                            if not game:GetService("Workspace")[game.Players.LocalPlayer.Name]:FindFirstChild("Zoan") then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "P", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "P", false, game)
                                                            end
                                                        end
                                                    end)
                                                end)
                                            end
                                            AutoHaki()
                                            if v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 50 then
                                                if FarmTween then FarmTween:Stop() end
                                                if _G.Settings.Config["Auto Skill"] then
                                                    PositionSkillMasteryDevilFruit = v.HumanoidRootPart.Position
                                                    spawn(function()
                                                        pcall(function()
                                                            if _G.Settings.Config["Use Skill"].Z == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "Z", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "Z", false, game)
                                                            end
                                                            if _G.Settings.Config["Use Skill"].X == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
                                                            end
                                                            if _G.Settings.Config["Use Skill"].C == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "C", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "C", false, game)
                                                            end
                                                            if _G.Settings.Config["Use Skill"].V == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "V", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "V", false, game)
                                                            end
                                                            if _G.Settings.Config["Use Skill"].B == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "B", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "B", false, game)
                                                            end
                                                        end)
                                                    end)
                                                end
                                                if v.Humanoid:FindFirstChild("Animator") then
                                                    v.Humanoid.Animator:Destroy()
                                                end
                                                v.HumanoidRootPart.CanCollide = false
                                                v.Head.CanCollide = false
                                                if _G.Settings.Config["Farm Mode"] == "Under" then
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) - Vector3.new(0,_G.Settings.Config["Distance Enemy"],0)
                                                elseif _G.Settings.Config["Farm Mode"] == "Above" then
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"])
                                                elseif  _G.Settings.Config["Farm Mode"] == "Behind" then 
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"])
                                                end
                                                EquipWeapon(_G.Settings.Config["Select Weapon"])
                                                game:GetService('VirtualUser'):CaptureController()
                                                game:GetService('VirtualUser'):Button1Down(Vector2.new(1280, 672))
                                            end
                                        until v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or not _G.Settings.Main["Auto Farm Level"] or game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.MainFrame.Frame.Main.List.QuestFrame.Progress.Progress.Text == "0/0"
                                    else
                                        Questtween = toTargetP(CFrameQuest.Position,CFrameQuest)
                                        if (CFrameQuest.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 150 then
                                            if Questtween then Questtween:Stop() end
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQuest
                                        end
                                    end
                                end
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    if World1 then
        tableMon = {"[Lv 1] Thief","[Lv 10] Bandit","[Lv 40] Pirate Clown","[Lv 90] Fishman","[Lv 160] Desert Thief","[Lv 250] Krieg Pirate","[Lv 350] Marine Recruit","[Lv 450] Monkey","[Lv 500] Gorilla","[Lv 600] Marine Grunt","[Lv 700] Satyr","[Lv 800] Elite Marine","[Lv 950] Sandorian Warrior","[Lv 1000] Divine Soldier","[Lv 1050] Holy Soldier","[Lv 1150] Revolutionary","[Lv 1200] Revolutionary Elite","[Lv 1300] Impel Guard","[Lv 1350] Impel Elite","[Lv 1450] Corrupt Marine","[Lv 1600] Island Fishman","[Lv 1650] Fishman Elite","[Lv 1750] Skull Pirate","[Lv 1800] Pirate Elite","[Lv 1900] Pirate","[Lv 1950] Armoured Marine","[Lv 2050] Skeleton","[Lv 2100] Mummy"}
    elseif World2 then
        tableMon = {"[Lv 2200] Samurai","[Lv 2250] Strong Samurai","[Lv 2300] Shinobi","[Lv 2350] Shinobi Elite","[Lv 2400] Officer Guard","[Lv 2450] Sergeant Guard","[Lv 2550] Graverobber","[Lv 2600] Elite Graverobber","[Lv 2700] Desert Pirate","[Lv 2750] Desert Captain","[Lv 2850] Beast Pirate","[Lv 2900] Elite Beast","[Lv 3150] Peanut Pirate","[Lv 3200] Peanut Captain","[Lv 3300] Dough Pirate","[Lv 3350] Dough Captain","[Lv 3450] Chocolate Pirate","[Lv 3500] Candy Guard"}
    end
    SelectMonster = ""
    local MonsterFarm = Tabs.Main:AddSection("[ Monster Farm ]")
    MonsterFarm:AddDropdown("MonsterSelector", {
		Title = "Select Monster to Farm",
		Values = tableMon,
		Multi = false,
		Default = SelectMonster,
	})
	Options.MonsterSelector:OnChanged(function()
		SelectMonster = Options["MonsterSelector"].Value
	end)

    MonsterFarm:AddToggle("MonsterFarm", {Title = "Auto Farm Selected Monster", Default = _G.Settings.Main["Auto Farm Selected Monster"]})
    Options.MonsterFarm:OnChanged(function()
        _G.Settings.Main["Auto Farm Selected Monster"] = Options["MonsterFarm"].Value
        if Options["MonsterFarm"].Value == false then
            wait()
            toTargetP(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
            wait()
        end
        SaveSettings()
    end)

    spawn(function()
        while task.wait() do
            if _G.Settings.Main["Auto Farm Selected Monster"] then
                pcall(function()
                    CheckLevel()
                    if game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.MainFrame.Frame.Main.List.QuestFrame.Progress.Progress.Text == "0/0" then
                        game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.QuestFunction:InvokeServer(workspace.Npc_Workspace.QuestGivers[NPCQuest], LevelQuest)
                    else
                        CheckLevel()
                        for i,v in pairs(game:GetService("Workspace")["NPC Zones"][IslandMon].NPCS:GetChildren()) do
                            if string.find(v.Name,NameMon) then
                                if v:FindFirstChild("NPCName").Value == NameMon then
                                    if v:FindFirstChild("HumanoidRootPart") then
                                        repeat task.wait()
                                            if _G.Settings.Config["Farm Mode"] == "Under" then
                                                FarmTween = toTargetP(v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                            elseif _G.Settings.Config["Farm Mode"] == "Above" then
                                                FarmTween = toTargetP(v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                            elseif  _G.Settings.Config["Farm Mode"] == "Behind" then 
                                                FarmTween = toTargetP(v.HumanoidRootPart.CFrame * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                            end
                                            if _G.Settings.Config["Auto Transform Gear"] then
                                                spawn(function()
                                                    pcall(function()
                                                        if _G.Settings.Config["Gear List"] == "Gear 4" then
                                                            if not game:GetService("Workspace")[game.Players.LocalPlayer.Name]:FindFirstChild("Gear4IsOn") then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "N", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "N", false, game)
                                                            end
                                                        elseif _G.Settings.Config["Gear List"] == "Gear 5" then
                                                            if not game:GetService("Workspace")[game.Players.LocalPlayer.Name]:FindFirstChild("Zoan") then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "P", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "P", false, game)
                                                            end
                                                        end
                                                    end)
                                                end)
                                            end
                                            AutoHaki()
                                            if v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 50 then
                                                if FarmTween then FarmTween:Stop() end
                                                if _G.Settings.Config["Auto Skill"] then
                                                    PositionSkillMasteryDevilFruit = v.HumanoidRootPart.Position
                                                    spawn(function()
                                                        pcall(function()
                                                            if _G.Settings.Config["Use Skill"].Z == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "Z", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "Z", false, game)
                                                            end
                                                            if _G.Settings.Config["Use Skill"].X == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
                                                            end
                                                            if _G.Settings.Config["Use Skill"].C == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "C", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "C", false, game)
                                                            end
                                                            if _G.Settings.Config["Use Skill"].V == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "V", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "V", false, game)
                                                            end
                                                            if _G.Settings.Config["Use Skill"].B == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                game:service('VirtualInputManager'):SendKeyEvent(true, "B", false, game)
                                                                wait(.1)
                                                                game:service('VirtualInputManager'):SendKeyEvent(false, "B", false, game)
                                                            end
                                                        end)
                                                    end)
                                                end
                                                if v.Humanoid:FindFirstChild("Animator") then
                                                    v.Humanoid.Animator:Destroy()
                                                end
                                                v.HumanoidRootPart.CanCollide = false
                                                v.Head.CanCollide = false
                                                if _G.Settings.Config["Farm Mode"] == "Under" then
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) - Vector3.new(0,_G.Settings.Config["Distance Enemy"],0)
                                                elseif _G.Settings.Config["Farm Mode"] == "Above" then
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"])
                                                elseif  _G.Settings.Config["Farm Mode"] == "Behind" then 
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"])
                                                end
                                                EquipWeapon(_G.Settings.Config["Select Weapon"])
                                                game:GetService('VirtualUser'):CaptureController()
                                                game:GetService('VirtualUser'):Button1Down(Vector2.new(1280, 672))
                                            end
                                        until v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or not _G.Settings.Main["Auto Farm Selected Monster"] or game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.MainFrame.Frame.Main.List.QuestFrame.Progress.Progress.Text == "0/0"
                                
                                    else
                                        Questtween = toTargetP(CFrameQuest.Position,CFrameQuest)
                                        if (CFrameQuest.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 150 then
                                            if Questtween then Questtween:Stop() end
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQuest
                                        end
                                    end
                                end
                            end
                        end
                    end
                end)
            end
        end
    end)
    
