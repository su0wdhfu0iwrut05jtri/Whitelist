		ThunderNotification({
			Title = "Key System",
			Content = "Success Copied to Clipboard , if not Copied try copy on Textbox",
			Delay = 5
		})
		KEY_ENTER.Text = PandaAuth:GetLink(service_name)
		wait(1)
		TextBtn.Text = "GET KEY"
	end)
end

task.spawn(function()
	pcall(function()
		local queueteleport = syn and syn.queue_on_teleport or queue_on_teleport or fluxus and fluxus.queue_on_teleport
		local hasExistingKey = isfile("THUNDERZKey.txt")
		if hasExistingKey and hasExistingKey ~= nil then
			print('attempting to load key...')
			local foundKey = readfile("THUNDERZKey.txt")
			if PandaAuth:ValidateKey(service_name, foundKey) then
				print("Key Valid")
				ThunderNotification({
					Title = "Key System",
					Content = "Found Existing Key ! , Load Script",
					Delay = 2
				})
				LoadScript()
			else
				print("Key Invalid")
				ThunderNotification({
					Title = "Key System",
					Content = "Expired Key.",
					Delay = 3
				})
				KeySystem()
			end
		elseif not hasExistingKey then
			KeySystem()
		end
	end)
end)


-- [Save Settings]

function LoadScript()
    _G.Settings = {
        Main = {
            --_G.Settings.Main[""]
            ["Auto Farm Level"] = false,
            ["Ignore Boss"] = false,
            ["Auto Second Sea"] = false,
            ["Auto Farm Selected Monster"] = false,
            ["Select Boss"] = 1,
            ["Auto Farm Boss"] = false,
            ["Island"] = 1,
            ["NPC"] = 1,
            ["Mode TP"] = 1,
            ["Island Toggle"] = false,
            ["NPC Toggle"] = false,
            ["Auto Whitebeard"] = false,
