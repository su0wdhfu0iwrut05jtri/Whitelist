                                        elseif string.find(v.Name,"Ice Admiral") == nil then
                                            toTargetP(CFrame.new(-6860, 233, 6864))
                                        end
                                    end
                                end
                            elseif game.Players.LocalPlayer.Backpack:FindFirstChild("Tremor Key") or game.Players.LocalPlayer.Character:FindFirstChild("Tremor Key") then
                                if (CFrame.new(-6347, 21, 6426).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 100 then
                                    if game.Players.LocalPlayer.Backpack:FindFirstChild("Tremor Key") then
                                        local tool = game.Players.LocalPlayer.Backpack:FindFirstChild("Tremor Key")
                                        wait(.4)
                                        game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
                                    end
                                    TremorDoor = toTargetP(CFrame.new(-6347.02441, 21.7708321, 6426.69336))
                                    game:GetService("Players").LocalPlayer.Character:FindFirstChild("Tremor Key").Spawn:InvokeServer()
                                    if (CFrame.new(-6347.02441, 21.7708321, 6426.69336).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 50 then
                                        if TremorDoor then TremorDoor:Stop() end
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-6347.02441, 21.7708321, 6426.69336)
                                        if (CFrame.new(-6347.02441, 21.7708321, 6426.69336).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 5 then
                                            game:GetService("Players").LocalPlayer.Character:FindFirstChild("Tremor Key").Spawn:InvokeServer()
                                        end
                                    end
                                end      
                            end
                        end
                    else
                        Fluent:Notify({
                            Title = "Thunder Z Hub",
                            Content = "Must Level 900 !",
                            Duration = 3
                        })
                        wait(3)
                    end
                end)
            end
        end
    end)

    SpawnBoss:AddToggle("DragonBoss", {Title = "Auto Spawn Kaido Boss", Default = _G.Settings.Main["Spawn Kaido"]})
    Options.DragonBoss:OnChanged(function()
        _G.Settings.Main["Spawn Kaido"] = Options["DragonBoss"].Value
        if Options["DragonBoss"].Value == false then
            wait()
            toTargetP(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
            wait()
        end
        SaveSettings()
    end)

    task.spawn(function()
        while task.wait() do
            if _G.Settings.Main["Spawn Kaido"] and World2 then
                pcall(function()
                    if game:GetService("Players").LocalPlayer.PlayerData.Experience.Level.Value >= 2950 then
                        game:GetService("ReplicatedStorage").Replication.ClientEvents.SetSpawnPoint:FireServer("Dragon Island")
                        if game:GetService("Workspace")["NPC Zones"].SpecialBosses.NPCS:FindFirstChildWhichIsA("Model") then
                            print("Ada Kaido")
                            for i1,v1 in pairs(game:GetService("Workspace")["NPC Zones"].SpecialBosses.NPCS:GetChildren()) do
                                if string.find(v1.Name,"DragonBoss") then
                                    print("memukul Kaido")
                                    BossTween = toTargetP(CFrame.new(7039, 1627, 6591))
                                    BossPos = CFrame.new(7039, 1627, 6591)
                                    if (BossPos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 200 or v1:FindFirstChild("HumanoidRootPart") then
                                        if BossTween then BossTween:Stop() end
                                    end
                                    repeat wait()
                                        if _G.Settings.Config["Farm Mode"] == "Under" then
                                            FarmTween = toTargetP(v1.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                        elseif _G.Settings.Config["Farm Mode"] == "Above" then
