                                TremorDoor = toTargetP(CFrame.new(-2117, 45, -2942))
                                game:GetService("Workspace").Npc_Workspace["Sea Teleporter"]["Sea 2"].UnlockSea:InvokeServer()
                                game:GetService("Workspace")["Npc_Workspace"]["Sea Teleporter"]["Sea 2"].Teleport:InvokeServer()
                                if (CFrame.new(-2117, 45, -2942).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 50 then
                                    if TremorDoor then TremorDoor:Stop() end
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2117, 45, -2942)
                                    if (CFrame.new(-2117, 45, -2942).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 5 then
                                        game:GetService("Workspace").Npc_Workspace["Sea Teleporter"]["Sea 2"].UnlockSea:InvokeServer()
                                        game:GetService("Workspace")["Npc_Workspace"]["Sea Teleporter"]["Sea 2"].Teleport:InvokeServer()
                                    end
                                end
                            end
                        end       
                    else
                        Fluent:Notify({
                            Title = "Thunder Z Hub",
                            Content = "Must Level 2200 !",
                            Duration = 3
                        })
                        wait(3)
                    end
                end)
            end
        end
    end)


