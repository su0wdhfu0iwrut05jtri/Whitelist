                                                                    game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
                                                                end
                                                                if _G.Settings.Config["Use Skill"].C == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                    game:service('VirtualInputManager'):SendKeyEvent(true, "C", false, game)
                                                                    wait(.1)
                                                                    game:service('VirtualInputManager'):SendKeyEvent(false, "C", false, game)
                                                                end
                                                                if _G.Settings.Config["Use Skill"].V == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                    game:service('VirtualInputManager'):SendKeyEvent(true, "V", false, game)
                                                                    wait(.1)
                                                                    game:service('VirtualInputManager'):SendKeyEvent(false, "V", false, game)
                                                                end
                                                                if _G.Settings.Config["Use Skill"].B == true and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                                    game:service('VirtualInputManager'):SendKeyEvent(true, "B", false, game)
                                                                    wait(.1)
                                                                    game:service('VirtualInputManager'):SendKeyEvent(false, "B", false, game)
                                                                end
                                                            end)
                                                        end)
                                                    end
                                                    if _G.Settings.Config["Farm Mode"] == "Under" then
                                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) - Vector3.new(0,_G.Settings.Config["Distance Enemy"],0)
                                                    elseif _G.Settings.Config["Farm Mode"] == "Above" then
                                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"])
                                                    elseif  _G.Settings.Config["Farm Mode"] == "Behind" then 
                                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"])
                                                    end
                                                    EquipWeapon(_G.Settings.Config["Select Weapon"])
                                                    game:GetService('VirtualUser'):CaptureController()
                                                    game:GetService('VirtualUser'):Button1Down(Vector2.new(1280, 672))
                                                end
                                            until v.Humanoid.Health <= 0 or not v.Parent or not v:FindFirstChild("HumanoidRootPart") or not _G.Settings.Main["Spawn Kaido"] or game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.MainFrame.Frame.Main.List.QuestFrame.Progress.Progress.Text == "0/0"
                                        elseif string.find(v.Name,"MaceV2 Boss") == nil then
                                            toTargetP(CFrame.new(7226, 1188, 6493))
                                        end
                                    end
                                end
                            elseif game.Players.LocalPlayer.Backpack:FindFirstChild("Dragon Orb") or game.Players.LocalPlayer.Character:FindFirstChild("Dragon Orb") then
                                if not game.Players.LocalPlayer.Character:FindFirstChild("Dragon Orb") then
                                    if game.Players.LocalPlayer.Backpack:FindFirstChild("Dragon Orb") then
                                        local tool = game.Players.LocalPlayer.Backpack:FindFirstChild("Dragon Orb")
                                        wait(.4)
                                        game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
                                    end
                                else
                                    game:GetService("Players").LocalPlayer.Character:FindFirstChild("Dragon Orb").Spawn:InvokeServer()
                                end      
                            elseif game.Players.LocalPlayer.Character:FindFirstChild("Drums of Liberation") or game.Players.LocalPlayer.Backpack:FindFirstChild("Drums of Liberation") then
                                toTargetP(CFrame.new(7051, 1110, 8048))
                                if game.Players.LocalPlayer.Backpack:FindFirstChild("Drums of Liberation") then
                                    local tool = game.Players.LocalPlayer.Backpack:FindFirstChild("Drums of Liberation")
                                    wait(.4)
                                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
                                end
                                game:GetService("Players").LocalPlayer.Character:FindFirstChild("Drums of Liberation").Request:InvokeServer("Store")                                
                            end
                        end
                    else
                        Fluent:Notify({
                            Title = "Thunder Z Hub",
                            Content = "Must Level 2950 !",
                            Duration = 3
                        })
                        wait(3)
                    end
                end)
            end
        end
    end)

    if World2 then
        SpawnBoss:AddToggle("SeaBeast", {Title = "Spawn Seabeast", Default = _G.Settings.Main["Auto Seabeast"]})
        Options.SeaBeast:OnChanged(function()
            _G.Settings.Main["Auto Seabeast"] = Options["SeaBeast"].Value
            if Options["SeaBeast"].Value == false then
                wait()
                toTargetP(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
                wait()
            end
            SaveSettings()
        end)
    else
        _G.Settings.Main["Auto Seabeast"] = false
    end
    
    spawn(function()
        while task.wait() do
            if _G.Settings.Main["Auto Seabeast"] and World2 then
                pcall(function()
                    if game:GetService("Workspace")["NPC Zones"].Monsters.NPCS:FindFirstChildOfClass("Model") then
                        for i1,v1 in pairs(game:GetService("Workspace")["NPC Zones"].Monsters.NPCS:GetChildren()) do
                            if string.find(v1.Name,"Sea Beast") then
                                if game:GetService("Workspace").BoatStorage:FindFirstChild(game.Players.LocalPlayer.Name) then
                                    game:GetService("Workspace").BoatStorage:FindFirstChild(game.Players.LocalPlayer.Name):Destroy()
                                end
                                if v1:FindFirstChild("NPCName").Value == "White Sea Beast" then -- v1:FindFirstChild("NPCName").Value == "White Sea Beast" --8025, 4, 13159
                                    BossTween = toTargetP(CFrame.new(200, 397, -3086))
                                    BossPos = CFrame.new(200, 397, -3086)
                                    if (BossPos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 200 or v1:FindFirstChild("HumanoidRootPart") then
                                        if BossTween then BossTween:Stop() end
                                    end
                                    repeat task.wait()
                                        if _G.Settings.Config["Farm Mode"] == "Under" then
                                            FarmTween = toTargetP(v1.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                        elseif _G.Settings.Config["Farm Mode"] == "Above" then
                                            FarmTween = toTargetP(v1.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                        elseif  _G.Settings.Config["Farm Mode"] == "Behind" then 
                                            FarmTween = toTargetP(v1.HumanoidRootPart.CFrame * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"]))
                                        end
                                        if _G.Settings.Config["Auto Transform Gear"] then
                                            spawn(function()
                                                pcall(function()
                                                    if _G.Settings.Config["Gear List"] == "Gear 4" then
                                                        if not game:GetService("Workspace")[game.Players.LocalPlayer.Name]:FindFirstChild("Gear4IsOn") then
                                                            game:service('VirtualInputManager'):SendKeyEvent(true, "N", false, game)
                                                            wait(.1)
                                                            game:service('VirtualInputManager'):SendKeyEvent(false, "N", false, game)
                                                        end
                                                    elseif _G.Settings.Config["Gear List"] == "Gear 5" then
                                                        if not game:GetService("Workspace")[game.Players.LocalPlayer.Name]:FindFirstChild("Zoan") then
                                                            game:service('VirtualInputManager'):SendKeyEvent(true, "P", false, game)
                                                            wait(.1)
                                                            game:service('VirtualInputManager'):SendKeyEvent(false, "P", false, game)
                                                        end
                                                    end
                                                end)
                                            end)
                                        end
                                        AutoHaki()
                                        if v1:FindFirstChild("HumanoidRootPart") and (v1.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 50 then
                                            if FarmTween then FarmTween:Stop() end
                                            if _G.Settings.Config["Auto Skill"] then
                                                PositionSkillMasteryDevilFruit = v1.HumanoidRootPart.Position
                                                spawn(function()
                                                    pcall(function()
                                                        if _G.Settings.Config["Use Skill"].Z == true and (v1.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                            game:service('VirtualInputManager'):SendKeyEvent(true, "Z", false, game)
                                                            wait(.1)
                                                            game:service('VirtualInputManager'):SendKeyEvent(false, "Z", false, game)
                                                        end
                                                        if _G.Settings.Config["Use Skill"].X == true and (v1.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                            game:service('VirtualInputManager'):SendKeyEvent(true, "X", false, game)
                                                            wait(.1)
                                                            game:service('VirtualInputManager'):SendKeyEvent(false, "X", false, game)
                                                        end
                                                        if _G.Settings.Config["Use Skill"].C == true and (v1.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                            game:service('VirtualInputManager'):SendKeyEvent(true, "C", false, game)
                                                            wait(.1)
                                                            game:service('VirtualInputManager'):SendKeyEvent(false, "C", false, game)
                                                        end
                                                        if _G.Settings.Config["Use Skill"].V == true and (v1.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                            game:service('VirtualInputManager'):SendKeyEvent(true, "V", false, game)
                                                            wait(.1)
                                                            game:service('VirtualInputManager'):SendKeyEvent(false, "V", false, game)
                                                        end
                                                        if _G.Settings.Config["Use Skill"].B == true and (v1.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.Settings.Config["Distance Enemy"] then
                                                            game:service('VirtualInputManager'):SendKeyEvent(true, "B", false, game)
                                                            wait(.1)
                                                            game:service('VirtualInputManager'):SendKeyEvent(false, "B", false, game)
                                                        end
                                                    end)
                                                end)
                                            end
                                            if _G.Settings.Config["Farm Mode"] == "Under" then
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v1.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), 0, 0) - Vector3.new(0,_G.Settings.Config["Distance Enemy"],0)
                                            elseif _G.Settings.Config["Farm Mode"] == "Above" then
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v1.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"])
                                            elseif  _G.Settings.Config["Farm Mode"] == "Behind" then 
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v1.HumanoidRootPart.CFrame * CFrame.new(0,0,_G.Settings.Config["Distance Enemy"])
                                            end
                                            EquipWeapon(_G.Settings.Config["Select Weapon"])
                                            game:GetService('VirtualUser'):CaptureController()
                                            game:GetService('VirtualUser'):Button1Down(Vector2.new(1280, 672))
                                        end
                                    until v1.Humanoid.Health <= 0 or not v1.Parent or not v1:FindFirstChild("HumanoidRootPart") or not _G.Settings.Main["Auto Seabeast"]
                                end
                            end
                        end
                    elseif not game:GetService("Workspace")["NPC Zones"].Monsters.NPCS:FindFirstChildOfClass("Model") then
                        if not game:GetService("Workspace").BoatStorage:FindFirstChild(game.Players.LocalPlayer.Name) then
                            buyb = toTargetP(CFrame.new(7890, 20, 11776))
                            if (CFrame.new(7890, 20, 11776).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                                if buyb then buyb:Stop() end wait(.1)
                                game:GetService("ReplicatedStorage").Replication.ClientEvents.BoatSeller_Event:InvokeServer(workspace.Npc_Workspace.BoatSellers["Desert City"], "Galleon")
                            end
                        else
                            if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
                                toTargetP(game:GetService("Workspace").BoatStorage:FindFirstChild(game.Players.LocalPlayer.Name).Drive.CFrame * CFrame.new(0,1,0))
                                print("Sui")
                            else
                                repeat wait()
                                    if (CFrame.new(13107, 36, 11802).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude >= 3000 then
                                        game:GetService("VirtualInputManager"):SendKeyEvent(true,"W",false,game)
                                    elseif (CFrame.new(13107, 36, 11802).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 1500 then
                                        game:GetService("VirtualInputManager"):SendKeyEvent(false,"W",false,game)
                                    end
                                until game:GetService("Workspace")["NPC Zones"].Monsters.NPCS:FindFirstChildOfClass("Model") or not _G.Settings.Main["Auto Seabeast"]
                            end
                        end
                    end
                    game:GetService("VirtualInputManager"):SendKeyEvent(false,"W",false,game)
                end)
            end
        end
    end)

